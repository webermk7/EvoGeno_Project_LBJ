[32m+[m[32m  theme( axis.text.x = element_text(size = 4, angle = 90)) +[m
[32m+[m[32m  theme(axis.line.y = element_line(linewidth = 0.25)) +[m
[32m+[m[32m  theme(axis.title = element_text(size = 5)) +[m
[32m+[m[32m  theme(axis.text.y = element_text(size = 4)) +[m
[32m+[m[32m  scale_x_discrete(labels = wrap_format(40)) +[m
[32m+[m[32m  scale_y_continuous(limits = c(0, 20)) +[m
[32m+[m[32m  xlab("Site") +[m
[32m+[m[32m  ylab("Count") +[m
[32m+[m[32m  labs(title = str_wrap("Actinobateriota Genera at each Site", width = 30)) +[m
[32m+[m[32m  ggeasy::easy_plot_title_size(size = 8) +[m
[32m+[m[32m  ggeasy::easy_center_title()[m
[32m+[m[32m```[m
