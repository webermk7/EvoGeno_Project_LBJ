[32m+[m[32m<script>(function() {[m
[32m+[m[32m  // If window.HTMLWidgets is already defined, then use it; otherwise create a[m
[32m+[m[32m  // new object. This allows preceding code to set options that affect the[m
[32m+[m[32m  // initialization process (though none currently exist).[m
[32m+[m[32m  window.HTMLWidgets = window.HTMLWidgets || {};[m
[32m+[m
[32m+[m[32m  // See if we're running in a viewer pane. If not, we're in a web browser.[m
[32m+[m[32m  var viewerMode = window.HTMLWidgets.viewerMode =[m
[32m+[m[32m      /\bviewer_pane=1\b/.test(window.location);[m
[32m+[m
[32m+[m[32m  // See if we're running in Shiny mode. If not, it's a static document.[m
[32m+[m[32m  // Note that static widgets can appear in both Shiny and static modes, but[m
[32m+[m[32m  // obviously, Shiny widgets can only appear in Shiny apps/documents.[m
[32m+[m[32m  var shinyMode = window.HTMLWidgets.shinyMode =[m
[32m+[m[32m      typeof(window.Shiny) !== "undefined" && !!window.Shiny.outputBindings;[m
[32m+[m
[32m+[m[32m  // We can't count on jQuery being available, so we implement our own[m
[32m+[m[32m  // version if necessary.[m
[32m+[m[32m  function querySelectorAll(scope, selector) {[m
[32m+[m[32m    if (typeof(jQuery) !== "undefined" && scope instanceof jQuery) {[m
[32m+[m[32m      return scope.find(selector);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (scope.querySelectorAll) {[m
[32m+[m[32m      return scope.querySelectorAll(selector);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function asArray(value) {[m
[32m+[m[32m    if (value === null)[m
[32m+[m[32m      return [];[m
[32m+[m[32m    if ($.isArray(value))[m
[32m+[m[32m      return value;[m
[32m+[m[32m    return [value];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Implement jQuery's extend[m
[32m+[m[32m  function extend(target /*, ... */) {[m
[32m+[m[32m    if (arguments.length == 1) {[m
[32m+[m[32m      return target;[m
[32m+[m[32m    }[m
[32m+[m[32m    for (var i = 1; i < arguments.length; i++) {[m
[32m+[m[32m      var source = arguments[i];[m
[32m+[m[32m      for (var prop in source) {[m
[32m+[m[32m        if (source.hasOwnProperty(prop)) {[m
[32m+[m[32m          target[prop] = source[prop];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return target;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // IE8 doesn't support Array.forEach.[m
[32m+[m[32m  function forEach(values, callback, thisArg) {[m
[32m+[m[32m    if (values.forEach) {[m
[32m+[m[32m      values.forEach(callback, thisArg);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      for (var i = 0; i < values.length; i++) {[m
[32m+[m[32m        callback.call(thisArg, values[i], i, values);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Replaces the specified method with the return value of funcSource.[m
[32m+[m[32m  //[m
[32m+[m[32m  // Note that funcSource should not BE the new method, it should be a function[m
[32m+[m[32m  // that RETURNS the new method. funcSource receives a single argument that is[m
[32m+[m[32m  // the overridden method, it can be called from the new method. The overridden[m
[32m+[m[32m  // method can be called like a regular function, it has the target permanently[m
[32m+[m[32m  // bound to it so "this" will work correctly.[m
[32m+[m[32m  function overrideMethod(target, methodName, funcSource) {[m
[32m+[m[32m    var superFunc = target[methodName] || function() {};[m
[32m+[m[32m    var superFuncBound = function() {[m
[32m+[m[32m      return superFunc.apply(target, arguments);[m
[32m+[m[32m    };[m
[32m+[m[32m    target[methodName] = funcSource(superFuncBound);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Add a method to delegator that, when invoked, calls[m
[32m+[m[32m  // delegatee.methodName. If there is no such method on[m
[32m+[m[32m  // the delegatee, but there was one on delegator before[m
[32m+[m[32m  // delegateMethod was called, then the original version[m
[32m+[m[32m  // is invoked instead.[m
[32m+[m[32m  // For example:[m
[32m+[m[32m  //[m
[32m+[m[32m  // var a = {[m
[32m+[m[32m  //   method1: function() { console.log('a1'); }[m
[32m+[m[32m  //   method2: function() { console.log('a2'); }[m
[32m+[m[32m  // };[m
[32m+[m[32m  // var b = {[m
[32m+[m[32m  //   method1: function() { console.log('b1'); }[m
[32m+[m[32m  // };[m
[32m+[m[32m  // delegateMethod(a, b, "method1");[m
[32m+[m[32m  // delegateMethod(a, b, "method2");[m
[32m+[m[32m  // a.method1();[m
[32m+[m[32m  // a.method2();[m
[32m+[m[32m  //[m
[32m+[m[32m  // The output would be "b1", "a2".[m
[32m+[m[32m  function delegateMethod(delegator, delegatee, methodName) {[m
[32m+[m[32m    var inherited = delegator[methodName];[m
[32m+[m[32m    delegator[methodName] = function() {[m
[32m+[m[32m      var target = delegatee;[m
[32m+[m[32m      var method = delegatee[methodName];[m
[32m+[m
[32m+[m[32m      // The method doesn't exist on the delegatee. Instead,[m
[32m+[m[32m      // call the method on the delegator, if it exists.[m
[32m+[m[32m      if (!method) {[m
[32m+[m[32m        target = delegator;[m
[32m+[m[32m        method = inherited;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (method) {[m
[32m+[m[32m        return method.apply(target, arguments);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Implement a vague facsimilie of jQuery's data method[m
[32m+[m[32m  function elementData(el, name, value) {[m
[32m+[m[32m    if (arguments.length == 2) {[m
[32m+[m[32m      return el["htmlwidget_data_" + name];[m
[32m+[m[32m    } else if (arguments.length == 3) {[m
[32m+[m[32m      el["htmlwidget_data_" + name] = value;[m
[32m+[m[32m      return el;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      throw new Error("Wrong number of arguments for elementData: " +[m
[32m+[m[32m        arguments.length);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // http://stackoverflow.com/questions/3446170/escape-string-for-use-in-javascript-regex[m
[32m+[m[32m  function escapeRegExp(str) {[m
[32m+[m[32m    return str.replace(/[\-\[\]\/\{\}\(\)\*\+\?\.\\\^\$\|]/g, "\\$&");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function hasClass(el, className) {[m
[32m+[m[32m    var re = new RegExp("\\b" + escapeRegExp(className) + "\\b");[m
[32m+[m[32m    return re.test(el.className);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // elements - array (or array-like object) of HTML elements[m
[32m+[m[32m  // className - class name to test for[m
[32m+[m[32m  // include - if true, only return elements with given className;[m
[32m+[m[32m  //   if false, only return elements *without* given className[m
[32m+[m[32m  function filterByClass(elements, className, include) {[m
[32m+[m[32m    var results = [];[m
[32m+[m[32m    for (var i = 0; i < elements.length; i++) {[m
[32m+[m[32m      if (hasClass(elements[i], className) == include)[m
[32m+[m[32m        results.push(elements[i]);[m
[32m+[m[32m    }[m
[32m+[m[32m    return results;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function on(obj, eventName, func) {[m
[32m+[m[32m    if (obj.addEventListener) {[m
[32m+[m[32m      obj.addEventListener(eventName, func, false);[m
[32m+[m[32m    } else if (obj.attachEvent) {[m
[32m+[m[32m      obj.attachEvent(eventName, func);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function off(obj, eventName, func) {[m
[32m+[m[32m    if (obj.removeEventListener)[m
[32m+[m[32m      obj.removeEventListener(eventName, func, false);[m
[32m+[m[32m    else if (obj.detachEvent) {[m
[32m+[m[32m      obj.detachEvent(eventName, func);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Translate array of values to top/right/bottom/left, as usual with[m
[32m+[m[32m  // the "padding" CSS property[m
[32m+[m[32m  // https://developer.mozilla.org/en-US/docs/Web/CSS/padding[m
[32m+[m[32m  function unpackPadding(value) {[m
[32m+[m[32m    if (typeof(value) === "number")[m
[32m+[m[32m      value = [value];[m
[32m+[m[32m    if (value.length === 1) {[m
[32m+[m[32m      return {top: value[0], right: value[0], bottom: value[0], left: value[0]};[m
[32m+[m[32m    }[m
[32m+[m[32m    if (value.length === 2) {[m
[32m+[m[32m      return {top: value[0], right: value[1], bottom: value[0], left: value[1]};[m
[32m+[m[32m    }[m
[32m+[m[32m    if (value.length === 3) {[m
[32m+[m[32m      return {top: value[0], right: value[1], bottom: value[2], left: value[1]};[m
[32m+[m[32m    }[m
[32m+[m[32m    if (value.length === 4) {[m
[32m+[m[32m      return {top: value[0], right: value[1], bottom: value[2], left: value[3]};[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Convert an unpacked padding object to a CSS value[m
[32m+[m[32m  function paddingToCss(paddingObj) {[m
[32m+[m[32m    return paddingObj.top + "px " + paddingObj.right + "px " + paddingObj.bottom + "px " + paddingObj.left + "px";[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Makes a number suitable for CSS[m
[32m+[m[32m  function px(x) {[m
[32m+[m[32m    if (typeof(x) === "number")[m
[32m+[m[32m      return x + "px";[m
[32m+[m[32m    else[m
[32m+[m[32m      return x;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Retrieves runtime widget sizing information for an element.[m
[32m+[m[32m  // The return value is either null, or an object with fill, padding,[m
[32m+[m[32m  // defaultWidth, defaultHeight fields.[m
[32m+[m[32m  function sizingPolicy(el) {[m
[32m+[m[32m    var sizingEl = document.querySelector("script[data-for='" + el.id + "'][type='application/htmlwidget-sizing']");[m
[32m+[m[32m    if (!sizingEl)[m
[32m+[m[32m      return null;[m
[32m+[m[32m    var sp = JSON.parse(sizingEl.textContent || sizingEl.text || "{}");[m
[32m+[m[32m    if (viewerMode) {[m
[32m+[m[32m      return sp.viewer;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return sp.browser;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // @param tasks Array of strings (or falsy value, in which case no-op).[m
[32m+[m[32m  //   Each element must be a valid JavaScript expression that yields a[m
[32m+[m[32m  //   function. Or, can be an array of objects with "code" and "data"[m
[32m+[m[32m  //   properties; in this case, the "code" property should be a string[m
[32m+[m[32m  //   of JS that's an expr that yields a function, and "data" should be[m
[32m+[m[32m  //   an object that will be added as an additional argument when that[m
[32m+[m[32m  //   function is called.[m
[32m+[m[32m  // @param target The object that will be "this" for each function[m
[32m+[m[32m  //   execution.[m
[32m+[m[32m  // @param args Array of arguments to be passed to the functions. (The[m
[32m+[m[32m  //   same arguments will be passed to all functions.)[m
[32m+[m[32m  function evalAndRun(tasks, target, args) {[m
[32m+[m[32m    if (tasks) {[m
[32m+[m[32m      forEach(tasks, function(task) {[m
[32m+[m[32m        var theseArgs = args;[m
[32m+[m[32m        if (typeof(task) === "object") {[m
[32m+[m[32m          theseArgs = theseArgs.concat([task.data]);[m
[32m+[m[32m          task = task.code;[m
[32m+[m[32m        }[m
[32m+[m[32m        var taskFunc = tryEval(task);[m
[32m+[m[32m        if (typeof(taskFunc) !== "function") {[m
[32m+[m[32m          throw new Error("Task must be a function! Source:\n" + task);[m
[32m+[m[32m        }[m
[32m+[m[32m        taskFunc.apply(target, theseArgs);[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Attempt eval() both with and without enclosing in parentheses.[m
[32m+[m[32m  // Note that enclosing coerces a function declaration into[m
[32m+[m[32m  // an expression that eval() can parse[m
[32m+[m[32m  // (otherwise, a SyntaxError is thrown)[m
[32m+[m[32m  function tryEval(code) {[m
[32m+[m[32m    var result = null;[m
[32m+[m[32m    try {[m
[32m+[m[32m      result = eval("(" + code + ")");[m
[32m+[m[32m    } catch(error) {[m
[32m+[m[32m      if (!(error instanceof SyntaxError)) {[m
[32m+[m[32m        throw error;[m
[32m+[m[32m      }[m
[32m+[m[32m      try {[m
[32m+[m[32m        result = eval(code);[m
[32m+[m[32m      } catch(e) {[m
[32m+[m[32m        if (e instanceof SyntaxError) {[m
[32m+[m[32m          throw error;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          throw e;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return result;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function initSizing(el) {[m
[32m+[m[32m    var sizing = sizingPolicy(el);[m
[32m+[m[32m    if (!sizing)[m
[32m+[m[32m      return;[m
[32m+[m
[32m+[m[32m    var cel = document.getElementById("htmlwidget_container");[m
[32m+[m[32m    if (!cel)[m
[32m+[m[32m      return;[m
[32m+[m
[32m+[m[32m    if (typeof(sizing.padding) !== "undefined") {[m
[32m+[m[32m      document.body.style.margin = "0";[m
[32m+[m[32m      document.body.style.padding = paddingToCss(unpackPadding(sizing.padding));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (sizing.fill) {[m
[32m+[m[32m      document.body.style.overflow = "hidden";[m
[32m+[m[32m      document.body.style.width = "100%";[m
[32m+[m[32m      document.body.style.height = "100%";[m
[32m+[m[32m      document.documentElement.style.width = "100%";[m
[32m+[m[32m      document.documentElement.style.height = "100%";[m
[32m+[m[32m      cel.style.position = "absolute";[m
[32m+[m[32m      var pad = unpackPadding(sizing.padding);[m
[32m+[m[32m      cel.style.top = pad.top + "px";[m
[32m+[m[32m      cel.style.right = pad.right + "px";[m
[32m+[m[32m      cel.style.bottom = pad.bottom + "px";[m
[32m+[m[32m      cel.style.left = pad.left + "px";[m
[32m+[m[32m      el.style.width = "100%";[m
[32m+[m[32m      el.style.height = "100%";[m
[32m+[m
[32m+[m[32m      return {[m
[32m+[m[32m        getWidth: function() { return cel.getBoundingClientRect().width; },[m
[32m+[m[32m        getHeight: function() { return cel.getBoundingClientRect().height; }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m    } else {[m
[32m+[m[32m      el.style.width = px(sizing.width);[m
[32m+[m[32m      el.style.height = px(sizing.height);[m
[32m+[m
[32m+[m[32m      return {[m
[32m+[m[32m        getWidth: function() { return cel.getBoundingClientRect().width; },[m
[32m+[m[32m        getHeight: function() { return cel.getBoundingClientRect().height; }[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Default implementations for methods[m
[32m+[m[32m  var defaults = {[m
[32m+[m[32m    find: function(scope) {[m
[32m+[m[32m      return querySelectorAll(scope, "." + this.name);[m
[32m+[m[32m    },[m
[32m+[m[32m    renderError: function(el, err) {[m
[32m+[m[32m      var $el = $(el);[m
[32m+[m
[32m+[m[32m      this.clearError(el);[m
[32m+[m
[32m+[m[32m      // Add all these error classes, as Shiny does[m
[32m+[m[32m      var errClass = "shiny-output-error";[m
[32m+[m[32m      if (err.type !== null) {[m
[32m+[m[32m        // use the classes of the error condition as CSS class names[m
[32m+[m[32m        errClass = errClass + " " + $.map(asArray(err.type), function(type) {[m
[32m+[m[32m          return errClass + "-" + type;[m
[32m+[m[32m        }).join(" ");[m
[32m+[m[32m      }[m
[32m+[m[32m      errClass = errClass + " htmlwidgets-error";[m
[32m+[m
[32m+[m[32m      // Is el inline or block? If inline or inline-block, just display:none it[m
[32m+[m[32m      // and add an inline error.[m
[32m+[m[32m      var display = $el.css("display");[m
[32m+[m[32m      $el.data("restore-display-mode", display);[m
[32m+[m
[32m+[m[32m      if (display === "inline" || display === "inline-block") {[m
[32m+[m[32m        $el.hide();[m
[32m+[m[32m        if (err.message !== "") {[m
[32m+[m[32m          var errorSpan = $("<span>").addClass(errClass);[m
[32m+[m[32m          errorSpan.text(err.message);[m
[32m+[m[32m          $el.after(errorSpan);[m
[32m+[m[32m        }[m
[32m+[m[32m      } else if (display === "block") {[m
[32m+[m[32m        // If block, add an error just after the el, set visibility:none on the[m
[32m+[m[32m        // el, and position the error to be on top of the el.[m
[32m+[m[32m        // Mark it with a unique ID and CSS class so we can remove it later.[m
[32m+[m[32m        $el.css("visibility", "hidden");[m
[32m+[m[32m        if (err.message !== "") {[m
[32m+[m[32m          var errorDiv = $("<div>").addClass(errClass).css("position", "absolute")[m
[32m+[m[32m            .css("top", el.offsetTop)[m
[32m+[m[32m            .css("left", el.offsetLeft)[m
[32m+[m[32m            // setting width can push out the page size, forcing otherwise[m
[32m+[m[32m            // unnecessary scrollbars to appear and making it impossible for[m
[32m+[m[32m            // the element to shrink; so use max-width instead[m
[32m+[m[32m            .css("maxWidth", el.offsetWidth)[m
[32m+[m[32m            .css("height", el.offsetHeight);[m
[32m+[m[32m          errorDiv.text(err.message);[m
[32m+[m[32m          $el.after(errorDiv);[m
[32m+[m
[32m+[m[32m          // Really dumb way to keep the size/position of the error in sync with[m
[32m+[m[32m          // the parent element as the window is resized or whatever.[m
[32m+[m[32m          var intId = setInterval(function() {[m
[32m+[m[32m            if (!errorDiv[0].parentElement) {[m
[32m+[m[32m              clearInterval(intId);[m
[32m+[m[32m              return;[m
[32m+[m[32m            }[m
[32m+[m[32m            errorDiv[m
[32m+[m[32m              .css("top", el.offsetTop)[m
[32m+[m[32m              .css("left", el.offsetLeft)[m
[32m+[m[32m              .css("maxWidth", el.offsetWidth)[m
[32m+[m[32m              .css("height", el.offsetHeight);[m
[32m+[m[32m          }, 500);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    clearError: function(el) {[m
[32m+[m[32m      var $el = $(el);[m
[32m+[m[32m      var display = $el.data("restore-display-mode");[m
[32m+[m[32m      $el.data("restore-display-mode", null);[m
[32m+[m
[32m+[m[32m      if (display === "inline" || display === "inline-block") {[m
[32m+[m[32m        if (display)[m
[32m+[m[32m          $el.css("display", display);[m
[32m+[m[32m        $(el.nextSibling).filter(".htmlwidgets-error").remove();[m
[32m+[m[32m      } else if (display === "block"){[m
[32m+[m[32m        $el.css("visibility", "inherit");[m
[32m+[m[32m        $(el.nextSibling).filter(".htmlwidgets-error").remove();[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    sizing: {}[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Called by widget bindings to register a new type of widget. The definition[m
[32m+[m[32m  // object can contain the following properties:[m
[32m+[m[32m  // - name (required) - A string indicating the binding name, which will be[m
[32m+[m[32m  //   used by default as the CSS classname to look for.[m
[32m+[m[32m  // - initialize (optional) - A function(el) that will be called once per[m
[32m+[m[32m  //   widget element; if a value is returned, it will be passed as the third[m
[32m+[m[32m  //   value to renderValue.[m
[32m+[m[32m  // - renderValue (required) - A function(el, data, initValue) that will be[m
[32m+[m[32m  //   called with data. Static contexts will cause this to be called once per[m
[32m+[m[32m  //   element; Shiny apps will cause this to be called multiple times per[m
[32m+[m[32m  //   element, as the data changes.[m
[32m+[m[32m  window.HTMLWidgets.widget = function(definition) {[m
[32m+[m[32m    if (!definition.name) {[m
[32m+[m[32m      throw new Error("Widget must have a name");[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!definition.type) {[m
[32m+[m[32m      throw new Error("Widget must have a type");[m
[32m+[m[32m    }[m
[32m+[m[32m    // Currently we only support output widgets[m
[32m+[m[32m    if (definition.type !== "output") {[m
[32m+[m[32m      throw new Error("Unrecognized widget type '" + definition.type + "'");[m
[32m+[m[32m    }[m
[32m+[m[32m    // TODO: Verify that .name is a valid CSS classname[m
[32m+[m
[32m+[m[32m    // Support new-style instance-bound definitions. Old-style class-bound[m
[32m+[m[32m    // definitions have one widget "object" per widget per type/class of[m
[32m+[m[32m    // widget; the renderValue and resize methods on such widget objects[m
[32m+[m[32m    // take el and instance arguments, because the widget object can't[m
[32m+[m[32m    // store them. New-style instance-bound definitions have one widget[m
[32m+[m[32m    // object per widget instance; the definition that's passed in doesn't[m
[32m+[m[32m    // provide renderValue or resize methods at all, just the single method[m
[32m+[m[32m    //   factory(el, width, height)[m
[32m+[m[32m    // which returns an object that has renderValue(x) and resize(w, h).[m
[32m+[m[32m    // This enables a far more natural programming style for the widget[m
[32m+[m[32m    // author, who can store per-instance state using either OO-style[m
[32m+[m[32m    // instance fields or functional-style closure variables (I guess this[m
[32m+[m[32m    // is in contrast to what can only be called C-style pseudo-OO which is[m
[32m+[m[32m    // what we required before).[m
[32m+[m[32m    if (definition.factory) {[m
[32m+[m[32m      definition = createLegacyDefinitionAdapter(definition);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!definition.renderValue) {[m
[32m+[m[32m      throw new Error("Widget must have a renderValue function");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // For static rendering (non-Shiny), use a simple widget registration[m
[32m+[m[32m    // scheme. We also use this scheme for Shiny apps/documents that also[m
[32m+[m[32m    // contain static widgets.[m
[32m+[m[32m    window.HTMLWidgets.widgets = window.HTMLWidgets.widgets || [];[m
[32m+[m[32m    // Merge defaults into the definition; don't mutate the original definition.[m
[32m+[m[32m    var staticBinding = extend({}, defaults, definition);[m
[32m+[m[32m    overrideMethod(staticBinding, "find", function(superfunc) {[m
[32m+[m[32m      return function(scope) {[m
[32m+[m[32m        var results = superfunc(scope);[m
[32m+[m[32m        // Filter out Shiny outputs, we only want the static kind[m
[32m+[m[32m        return filterByClass(results, "html-widget-output", false);[m
[32m+[m[32m      };[m
[32m+[m[32m    });[m
[32m+[m[32m    window.HTMLWidgets.widgets.push(staticBinding);[m
[32m+[m
[32m+[m[32m    if (shinyMode) {[m
[32m+[m[32m      // Shiny is running. Register the definition with an output binding.[m
[32m+[m[32m      // The definition itself will not be the output binding, instead[m
[32m+[m[32m      // we will make an output binding object that delegates to the[m
[32m+[m[32m      // definition. This is because we foolishly used the same method[m
[32m+[m[32m      // name (renderValue) for htmlwidgets definition and Shiny bindings[m
[32m+[m[32m      // but they actually have quite different semantics (the Shiny[m
[32m+[m[32m      // bindings receive data that includes lots of metadata that it[m
[32m+[m[32m      // strips off before calling htmlwidgets renderValue). We can't[m
[32m+[m[32m      // just ignore the difference because in some widgets it's helpful[m
[32m+[m[32m      // to call this.renderValue() from inside of resize(), and if[m
[32m+[m[32m      // we're not delegating, then that call will go to the Shiny[m
[32m+[m[32m      // version instead of the htmlwidgets version.[m
[32m+[m
[32m+[m[32m      // Merge defaults with definition, without mutating either.[m
[32m+[m[32m      var bindingDef = extend({}, defaults, definition);[m
[32m+[m
[32m+[m[32m      // This object will be our actual Shiny binding.[m
[32m+[m[32m      var shinyBinding = new Shiny.OutputBinding();[m
[32m+[m
[32m+[m[32m      // With a few exceptions, we'll want to simply use the bindingDef's[m
[32m+[m[32m      // version of methods if they are available, otherwise fall back to[m
[32m+[m[32m      // Shiny's defaults. NOTE: If Shiny's output bindings gain additional[m
[32m+[m[32m      // methods in the future, and we want them to be overrideable by[m
[32m+[m[32m      // HTMLWidget binding definitions, then we'll need to add them to this[m
[32m+[m[32m      // list.[m
[32m+[m[32m      delegateMethod(shinyBinding, bindingDef, "getId");[m
[32m+[m[32m      delegateMethod(shinyBinding, bindingDef, "onValueChange");[m
[32m+[m[32m      delegateMethod(shinyBinding, bindingDef, "onValueError");[m
[32m+[m[32m      delegateMethod(shinyBinding, bindingDef, "renderError");[m
[32m+[m[32m      delegateMethod(shinyBinding, bindingDef, "clearError");[m
[32m+[m[32m      delegateMethod(shinyBinding, bindingDef, "showProgress");[m
[32m+[m
[32m+[m[32m      // The find, renderValue, and resize are handled differently, because we[m
[32m+[m[32m      // want to actually decorate the behavior of the bindingDef methods.[m
[32m+[m
[32m+[m[32m      shinyBinding.find = function(scope) {[m
[32m+[m[32m        var results = bindingDef.find(scope);[m
[32m+[m
[32m+[m[32m        // Only return elements that are Shiny outputs, not static ones[m
[32m+[m[32m        var dynamicResults = results.filter(".html-widget-output");[m
[32m+[m
[32m+[m[32m        // It's possible that whatever caused Shiny to think there might be[m
[32m+[m[32m        // new dynamic outputs, also caused there to be new static outputs.[m
[32m+[m[32m        // Since there might be lots of different htmlwidgets bindings, we[m
[32m+[m[32m        // schedule execution for later--no need to staticRender multiple[m
[32m+[m[32m        // times.[m
[32m+[m[32m        if (results.length !== dynamicResults.length)[m
[32m+[m[32m          scheduleStaticRender();[m
[32m+[m
[32m+[m[32m        return dynamicResults;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      // Wrap renderValue to handle initialization, which unfortunately isn't[m
[32m+[m[32m      // supported natively by Shiny at the time of this writing.[m
[32m+[m
[32m+[m[32m      shinyBinding.renderValue = function(el, data) {[m
[32m+[m[32m        Shiny.renderDependencies(data.deps);[m
[32m+[m[32m        // Resolve strings marked as javascript literals to objects[m
[32m+[m[32m        if (!(data.evals instanceof Array)) data.evals = [data.evals];[m
[32m+[m[32m        for (var i = 0; data.evals && i < data.evals.length; i++) {[m
[32m+[m[32m          window.HTMLWidgets.evaluateStringMember(data.x, data.evals[i]);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (!bindingDef.renderOnNullValue) {[m
[32m+[m[32m          if (data.x === null) {[m
[32m+[m[32m            el.style.visibility = "hidden";[m
[32m+[m[32m            return;[m
[32m+[m[32m          } else {[m
[32m+[m[32m            el.style.visibility = "inherit";[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        if (!elementData(el, "initialized")) {[m
[32m+[m[32m          initSizing(el);[m
[32m+[m
[32m+[m[32m          elementData(el, "initialized", true);[m
[32m+[m[32m          if (bindingDef.initialize) {[m
[32m+[m[32m            var rect = el.getBoundingClientRect();[m
[32m+[m[32m            var result = bindingDef.initialize(el, rect.width, rect.height);[m
[32m+[m[32m            elementData(el, "init_result", result);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        bindingDef.renderValue(el, data.x, elementData(el, "init_result"));[m
[32m+[m[32m        evalAndRun(data.jsHooks.render, elementData(el, "init_result"), [el, data.x]);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      // Only override resize if bindingDef implements it[m
[32m+[m[32m      if (bindingDef.resize) {[m
[32m+[m[32m        shinyBinding.resize = function(el, width, height) {[m
[32m+[m[32m          // Shiny can call resize before initialize/renderValue have been[m
[32m+[m[32m          // called, which doesn't make sense for widgets.[m
[32m+[m[32m          if (elementData(el, "initialized")) {[m
[32m+[m[32m            bindingDef.resize(el, width, height, elementData(el, "init_result"));[m
[32m+[m[32m          }[m
[32m+[m[32m        };[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      Shiny.outputBindings.register(shinyBinding, bindingDef.name);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var scheduleStaticRenderTimerId = null;[m
[32m+[m[32m  function scheduleStaticRender() {[m
[32m+[m[32m    if (!scheduleStaticRenderTimerId) {[m
[32m+[m[32m      scheduleStaticRenderTimerId = setTimeout(function() {[m
[32m+[m[32m        scheduleStaticRenderTimerId = null;[m
[32m+[m[32m        window.HTMLWidgets.staticRender();[m
[32m+[m[32m      }, 1);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Render static widgets after the document finishes loading[m
[32m+[m[32m  // Statically render all elements that are of this widget's class[m
[32m+[m[32m  window.HTMLWidgets.staticRender = function() {[m
[32m+[m[32m    var bindings = window.HTMLWidgets.widgets || [];[m
[32m+[m[32m    forEach(bindings, function(binding) {[m
[32m+[m[32m      var matches = binding.find(document.documentElement);[m
[32m+[m[32m      forEach(matches, function(el) {[m
[32m+[m[32m        var sizeObj = initSizing(el, binding);[m
[32m+[m
[32m+[m[32m        var getSize = function(el) {[m
[32m+[m[32m          if (sizeObj) {[m
[32m+[m[32m            return {w: sizeObj.getWidth(), h: sizeObj.getHeight()}[m
[32m+[m[32m          } else {[m
[32m+[m[32m            var rect = el.getBoundingClientRect();[m
[32m+[m[32m            return {w: rect.width, h: rect.height}[m
[32m+[m[32m          }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (hasClass(el, "html-widget-static-bound"))[m
[32m+[m[32m          return;[m
[32m+[m[32m        el.className = el.className + " html-widget-static-bound";[m
[32m+[m
[32m+[m[32m        var initResult;[m
[32m+[m[32m        if (binding.initialize) {[m
[32m+[m[32m          var size = getSize(el);[m
[32m+[m[32m          initResult = binding.initialize(el, size.w, size.h);[m
[32m+[m[32m          elementData(el, "init_result", initResult);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (binding.resize) {[m
[32m+[m[32m          var lastSize = getSize(el);[m
[32m+[m[32m          var resizeHandler = function(e) {[m
[32m+[m[32m            var size = getSize(el);[m
[32m+[m[32m            if (size.w === 0 && size.h === 0)[m
[32m+[m[32m              return;[m
[32m+[m[32m            if (size.w === lastSize.w && size.h === lastSize.h)[m
[32m+[m[32m              return;[m
[32m+[m[32m            lastSize = size;[m
[32m+[m[32m            binding.resize(el, size.w, size.h, initResult);[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          on(window, "resize", resizeHandler);[m
[32m+[m
[32m+[m[32m          // This is needed for cases where we're running in a Shiny[m
[32m+[m[32m          // app, but the widget itself is not a Shiny output, but[m
[32m+[m[32m          // rather a simple static widget. One example of this is[m
[32m+[m[32m          // an rmarkdown document that has runtime:shiny and widget[m
[32m+[m[32m          // that isn't in a render function. Shiny only knows to[m
[32m+[m[32m          // call resize handlers for Shiny outputs, not for static[m
[32m+[m[32m          // widgets, so we do it ourselves.[m
[32m+[m[32m          if (window.jQuery) {[m
[32m+[m[32m            window.jQuery(document).on([m
[32m+[m[32m              "shown.htmlwidgets shown.bs.tab.htmlwidgets shown.bs.collapse.htmlwidgets",[m
[32m+[m[32m              resizeHandler[m
[32m+[m[32m            );[m
[32m+[m[32m            window.jQuery(document).on([m
[32m+[m[32m              "hidden.htmlwidgets hidden.bs.tab.htmlwidgets hidden.bs.collapse.htmlwidgets",[m
[32m+[m[32m              resizeHandler[m
[32m+[m[32m            );[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          // This is needed for the specific case of ioslides, which[m
[32m+[m[32m          // flips slides between display:none and display:block.[m
[32m+[m[32m          // Ideally we would not have to have ioslide-specific code[m
[32m+[m[32m          // here, but rather have ioslides raise a generic event,[m
[32m+[m[32m          // but the rmarkdown package just went to CRAN so the[m
[32m+[m[32m          // window to getting that fixed may be long.[m
[32m+[m[32m          if (window.addEventListener) {[m
[32m+[m[32m            // It's OK to limit this to window.addEventListener[m
[32m+[m[32m            // browsers because ioslides itself only supports[m
[32m+[m[32m            // such browsers.[m
[32m+[m[32m            on(document, "slideenter", resizeHandler);[m
[32m+[m[32m            on(document, "slideleave", resizeHandler);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var scriptData = document.querySelector("script[data-for='" + el.id + "'][type='application/json']");[m
[32m+[m[32m        if (scriptData) {[m
[32m+[m[32m          var data = JSON.parse(scriptData.textContent || scriptData.text);[m
[32m+[m[32m          // Resolve strings marked as javascript literals to objects[m
[32m+[m[32m          if (!(data.evals instanceof Array)) data.evals = [data.evals];[m
[32m+[m[32m          for (var k = 0; data.evals && k < data.evals.length; k++) {[m
[32m+[m[32m            window.HTMLWidgets.evaluateStringMember(data.x, data.evals[k]);[m
[32m+[m[32m          }[m
[32m+[m[32m          binding.renderValue(el, data.x, initResult);[m
[32m+[m[32m          evalAndRun(data.jsHooks.render, initResult, [el, data.x]);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    invokePostRenderHandlers();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  function has_jQuery3() {[m
[32m+[m[32m    if (!window.jQuery) {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m    var $version = window.jQuery.fn.jquery;[m
[32m+[m[32m    var $major_version = parseInt($version.split(".")[0]);[m
[32m+[m[32m    return $major_version >= 3;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /*[m
[32m+[m[32m  / Shiny 1.4 bumped jQuery from 1.x to 3.x which means jQuery's[m
[32m+[m[32m  / on-ready handler (i.e., $(fn)) is now asyncronous (i.e., it now[m
[32m+[m[32m  / really means $(setTimeout(fn)).[m
[32m+[m[32m  / https://jquery.com/upgrade-guide/3.0/#breaking-change-document-ready-handlers-are-now-asynchronous[m
[32m+[m[32m  /[m
[32m+[m[32m  / Since Shiny uses $() to schedule initShiny, shiny>=1.4 calls initShiny[m
[32m+[m[32m  / one tick later than it did before, which means staticRender() is[m
[32m+[m[32m  / called renderValue() earlier than (advanced) widget authors might be expecting.[m
[32m+[m[32m  / https://github.com/rstudio/shiny/issues/2630[m
[32m+[m[32m  /[m
[32m+[m[32m  / For a concrete example, leaflet has some methods (e.g., updateBounds)[m
[32m+[m[32m  / which reference Shiny methods registered in initShiny (e.g., setInputValue).[m
[32m+[m[32m  / Since leaflet is privy to this life-cycle, it knows to use setTimeout() to[m
[32m+[m[32m  / delay execution of those methods (until Shiny methods are ready)[m
[32m+[m[32m  / https://github.com/rstudio/leaflet/blob/18ec981/javascript/src/index.js#L266-L268[m
[32m+[m[32m  /[m
[32m+[m[32m  / Ideally widget authors wouldn't need to use this setTimeout() hack that[m
[32m+[m[32m  / leaflet uses to call Shiny methods on a staticRender(). In the long run,[m
[32m+[m[32m  / the logic initShiny should be broken up so that method registration happens[m
[32m+[m[32m  / right away, but binding happens later.[m
[32m+[m[32m  */[m
[32m+[m[32m  function maybeStaticRenderLater() {[m
[32m+[m[32m    if (shinyMode && has_jQuery3()) {[m
[32m+[m[32m      window.jQuery(window.HTMLWidgets.staticRender);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      window.HTMLWidgets.staticRender();[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (document.addEventListener) {[m
[32m+[m[32m    document.addEventListener("DOMContentLoaded", function() {[m
[32m+[m[32m      document.removeEventListener("DOMContentLoaded", arguments.callee, false);[m
[32m+[m[32m      maybeStaticRenderLater();[m
[32m+[m[32m    }, false);[m
[32m+[m[32m  } else if (document.attachEvent) {[m
[32m+[m[32m    document.attachEvent("onreadystatechange", function() {[m
[32m+[m[32m      if (document.readyState === "complete") {[m
[32m+[m[32m        document.detachEvent("onreadystatechange", arguments.callee);[m
[32m+[m[32m        maybeStaticRenderLater();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  window.HTMLWidgets.getAttachmentUrl = function(depname, key) {[m
[32m+[m[32m    // If no key, default to the first item[m
[32m+[m[32m    if (typeof(key) === "undefined")[m
[32m+[m[32m      key = 1;[m
[32m+[m
[32m+[m[32m    var link = document.getElementById(depname + "-" + key + "-attachment");[m
[32m+[m[32m    if (!link) {[m
[32m+[m[32m      throw new Error("Attachment " + depname + "/" + key + " not found in document");[m
[32m+[m[32m    }[m
[32m+[m[32m    return link.getAttribute("href");[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  window.HTMLWidgets.dataframeToD3 = function(df) {[m
[32m+[m[32m    var names = [];[m
[32m+[m[32m    var length;[m
[32m+[m[32m    for (var name in df) {[m
[32m+[m[32m        if (df.hasOwnProperty(name))[m
[32m+[m[32m            names.push(name);[m
[32m+[m[32m        if (typeof(df[name]) !== "object" || typeof(df[name].length) === "undefined") {[m
[32m+[m[32m            throw new Error("All fields must be arrays");[m
[32m+[m[32m        } else if (typeof(length) !== "undefined" && length !== df[name].length) {[m
[32m+[m[32m            throw new Error("All fields must be arrays of the same length");[m
[32m+[m[32m        }[m
[32m+[m[32m        length = df[name].length;[m
[32m+[m[32m    }[m
[32m+[m[32m    var results = [];[m
[32m+[m[32m    var item;[m
[32m+[m[32m    for (var row = 0; row < length; row++) {[m
[32m+[m[32m        item = {};[m
[32m+[m[32m        for (var col = 0; col < names.length; col++) {[m
[32m+[m[32m            item[names[col]] = df[names[col]][row];[m
[32m+[m[32m        }[m
[32m+[m[32m        results.push(item);[m
[32m+[m[32m    }[m
[32m+[m[32m    return results;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  window.HTMLWidgets.transposeArray2D = function(array) {[m
[32m+[m[32m      if (array.length === 0) return array;[m
[32m+[m[32m      var newArray = array[0].map(function(col, i) {[m
[32m+[m[32m          return array.map(function(row) {[m
[32m+[m[32m              return row[i][m
[32m+[m[32m          })[m
[32m+[m[32m      });[m
[32m+[m[32m      return newArray;[m
[32m+[m[32m  };[m
[32m+[m[32m  // Split value at splitChar, but allow splitChar to be escaped[m
[32m+[m[32m  // using escapeChar. Any other characters escaped by escapeChar[m
[32m+[m[32m  // will be included as usual (including escapeChar itself).[m
[32m+[m[32m  function splitWithEscape(value, splitChar, escapeChar) {[m
[32m+[m[32m    var results = [];[m
[32m+[m[32m    var escapeMode = false;[m
[32m+[m[32m    var currentResult = "";[m
[32m+[m[32m    for (var pos = 0; pos < value.length; pos++) {[m
[32m+[m[32m      if (!escapeMode) {[m
[32m+[m[32m        if (value[pos] === splitChar) {[m
[32m+[m[32m          results.push(currentResult);[m
[32m+[m[32m          currentResult = "";[m
[32m+[m[32m        } else if (value[pos] === escapeChar) {[m
[32m+[m[32m          escapeMode = true;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          currentResult += value[pos];[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        currentResult += value[pos];[m
[32m+[m[32m        escapeMode = false;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    if (currentResult !== "") {[m
[32m+[m[32m      results.push(currentResult);[m
[32m+[m[32m    }[m
[32m+[m[32m    return results;[m
[32m+[m[32m  }[m
[32m+[m[32m  // Function authored by Yihui/JJ Allaire[m
[32m+[m[32m  window.HTMLWidgets.evaluateStringMember = function(o, member) {[m
[32m+[m[32m    var parts = splitWithEscape(member, '.', '\\');[m
[32m+[m[32m    for (var i = 0, l = parts.length; i < l; i++) {[m
[32m+[m[32m      var part = parts[i];[m
[32m+[m[32m      // part may be a character or 'numeric' member name[m
[32m+[m[32m      if (o !== null && typeof o === "object" && part in o) {[m
[32m+[m[32m        if (i == (l - 1)) { // if we are at the end of the line then evalulate[m
[32m+[m[32m          if (typeof o[part] === "string")[m
[32m+[m[32m            o[part] = tryEval(o[part]);[m
[32m+[m[32m        } else { // otherwise continue to next embedded object[m
[32m+[m[32m          o = o[part];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Retrieve the HTMLWidget instance (i.e. the return value of an[m
[32m+[m[32m  // HTMLWidget binding's initialize() or factory() function)[m
[32m+[m[32m  // associated with an element, or null if none.[m
[32m+[m[32m  window.HTMLWidgets.getInstance = function(el) {[m
[32m+[m[32m    return elementData(el, "init_result");[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Finds the first element in the scope that matches the selector,[m
[32m+[m[32m  // and returns the HTMLWidget instance (i.e. the return value of[m
[32m+[m[32m  // an HTMLWidget binding's initialize() or factory() function)[m
[32m+[m[32m  // associated with that element, if any. If no element matches the[m
[32m+[m[32m  // selector, or the first matching element has no HTMLWidget[m
[32m+[m[32m  // instance associated with it, then null is returned.[m
[32m+[m[32m  //[m
[32m+[m[32m  // The scope argument is optional, and defaults to window.document.[m
[32m+[m[32m  window.HTMLWidgets.find = function(scope, selector) {[m
[32m+[m[32m    if (arguments.length == 1) {[m
[32m+[m[32m      selector = scope;[m
[32m+[m[32m      scope = document;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var el = scope.querySelector(selector);[m
[32m+[m[32m    if (el === null) {[m
[32m+[m[32m      return null;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return window.HTMLWidgets.getInstance(el);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Finds all elements in the scope that match the selector, and[m
[32m+[m[32m  // returns the HTMLWidget instances (i.e. the return values of[m
[32m+[m[32m  // an HTMLWidget binding's initialize() or factory() function)[m
[32m+[m[32m  // associated with the elements, in an array. If elements that[m
[32m+[m[32m  // match the selector don't have an associated HTMLWidget[m
[32m+[m[32m  // instance, the returned array will contain nulls.[m
[32m+[m[32m  //[m
[32m+[m[32m  // The scope argument is optional, and defaults to window.document.[m
[32m+[m[32m  window.HTMLWidgets.findAll = function(scope, selector) {[m
[32m+[m[32m    if (arguments.length == 1) {[m
[32m+[m[32m      selector = scope;[m
[32m+[m[32m      scope = document;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var nodes = scope.querySelectorAll(selector);[m
[32m+[m[32m    var results = [];[m
[32m+[m[32m    for (var i = 0; i < nodes.length; i++) {[m
[32m+[m[32m      results.push(window.HTMLWidgets.getInstance(nodes[i]));[m
[32m+[m[32m    }[m
[32m+[m[32m    return results;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var postRenderHandlers = [];[m
[32m+[m[32m  function invokePostRenderHandlers() {[m
[32m+[m[32m    while (postRenderHandlers.length) {[m
[32m+[m[32m      var handler = postRenderHandlers.shift();[m
[32m+[m[32m      if (handler) {[m
[32m+[m[32m        handler();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Register the given callback function to be invoked after the[m
[32m+[m[32m  // next time static widgets are rendered.[m
[32m+[m[32m  window.HTMLWidgets.addPostRenderHandler = function(callback) {[m
[32m+[m[32m    postRenderHandlers.push(callback);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Takes a new-style instance-bound definition, and returns an[m
[32m+[m[32m  // old-style class-bound definition. This saves us from having[m
[32m+[m[32m  // to rewrite all the logic in this file to accomodate both[m
[32m+[m[32m  // types of definitions.[m
[32m+[m[32m  function createLegacyDefinitionAdapter(defn) {[m
[32m+[m[32m    var result = {[m
[32m+[m[32m      name: defn.name,[m
[32m+[m[32m      type: defn.type,[m
[32m+[m[32m      initialize: function(el, width, height) {[m
[32m+[m[32m        return defn.factory(el, width, height);[m
[32m+[m[32m      },[m
[32m+[m[32m      renderValue: function(el, x, instance) {[m
[32m+[m[32m        return instance.renderValue(x);[m
[32m+[m[32m      },[m
[32m+[m[32m      resize: function(el, width, height, instance) {[m
[32m+[m[32m        return instance.resize(width, height);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (defn.find)[m
[32m+[m[32m      result.find = defn.find;[m
[32m+[m[32m    if (defn.renderError)[m
[32m+[m[32m      result.renderError = defn.renderError;[m
[32m+[m[32m    if (defn.clearError)[m
[32m+[m[32m      result.clearError = defn.clearError;[m
[32m+[m
[32m+[m[32m    return result;[m
[32m+[m[32m  }[m
[32m+[m[32m})();[m
[32m+[m[32m</script>[m
[32m+[m[32m<style type="text/css">.dt-crosstalk-fade {[m
[32m+[m[32mopacity: 0.2;[m
[32m+[m[32m}[m
[32m+[m[32mhtml body div.DTS div.dataTables_scrollBody {[m
[32m+[m[32mbackground: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtable.dataTable {[m
[32m+[m[32mdisplay: table;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.html-fill-container > .html-fill-item.datatables {[m
[32m+[m[32mflex-basis: 400px;[m
[32m+[m[32m}[m
[32m+[m[32m</style>[m
[32m+[m[32m<script>(function() {[m
[32m+[m
[32m+[m[32m// some helper functions: using a global object DTWidget so that it can be used[m
[32m+[m[32m// in JS() code, e.g. datatable(options = list(foo = JS('code'))); unlike R's[m
[32m+[m[32m// dynamic scoping, when 'code' is eval'ed, JavaScript does not know objects[m
[32m+[m[32m// from the "parent frame", e.g. JS('DTWidget') will not work unless it was made[m
[32m+[m[32m// a global object[m
[32m+[m[32mvar DTWidget = {};[m
[32m+[m
[32m+[m[32m// 123456666.7890 -> 123,456,666.7890[m
[32m+[m[32mvar markInterval = function(d, digits, interval, mark, decMark, precision) {[m
[32m+[m[32m  x = precision ? d.toPrecision(digits) : d.toFixed(digits);[m
[32m+[m[32m  if (!/^-?[\d.]+$/.test(x)) return x;[m
[32m+[m[32m  var xv = x.split('.');[m
[32m+[m[32m  if (xv.length > 2) return x;  // should have at most one decimal point[m
[32m+[m[32m  xv[0] = xv[0].replace(new RegExp('\\B(?=(\\d{' + interval + '})+(?!\\d))', 'g'), mark);[m
[32m+[m[32m  return xv.join(decMark);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mDTWidget.formatCurrency = function(data, currency, digits, interval, mark, decMark, before, zeroPrint) {[m
[32m+[m[32m  var d = parseFloat(data);[m
[32m+[m[32m  if (isNaN(d)) return '';[m
[32m+[m[32m  if (zeroPrint !== null && d === 0.0) return zeroPrint;[m
[32m+[m[32m  var res = markInterval(d, digits, interval, mark, decMark);[m
[32m+[m[32m  res = before ? (/^-/.test(res) ? '-' + currency + res.replace(/^-/, '') : currency + res) :[m
[32m+[m[32m    res + currency;[m
[32m+[m[32m  return res;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mDTWidget.formatString = function(data, prefix, suffix) {[m
[32m+[m[32m  var d = data;[m
[32m+[m[32m  if (d === null) return '';[m
[32m+[m[32m  return prefix + d + suffix;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mDTWidget.formatPercentage = function(data, digits, interval, mark, decMark, zeroPrint) {[m
[32m+[m[32m  var d = parseFloat(data);[m
[32m+[m[32m  if (isNaN(d)) return '';[m
[32m+[m[32m  if (zeroPrint !== null && d === 0.0) return zeroPrint;[m
[32m+[m[32m  return markInterval(d * 100, digits, interval, mark, decMark) + '%';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mDTWidget.formatRound = function(data, digits, interval, mark, decMark, zeroPrint) {[m
[32m+[m[32m  var d = parseFloat(data);[m
[32m+[m[32m  if (isNaN(d)) return '';[m
[32m+[m[32m  if (zeroPrint !== null && d === 0.0) return zeroPrint;[m
[32m+[m[32m  return markInterval(d, digits, interval, mark, decMark);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mDTWidget.formatSignif = function(data, digits, interval, mark, decMark, zeroPrint) {[m
[32m+[m[32m  var d = parseFloat(data);[m
[32m+[m[32m  if (isNaN(d)) return '';[m
[32m+[m[32m  if (zeroPrint !== null && d === 0.0) return zeroPrint;[m
[32m+[m[32m  return markInterval(d, digits, interval, mark, decMark, true);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mDTWidget.formatDate = function(data, method, params) {[m
[32m+[m[32m  var d = data;[m
[32m+[m[32m  if (d === null) return '';[m
[32m+[m[32m  // (new Date('2015-10-28')).toDateString() may return 2015-10-27 because the[m
[32m+[m[32m  // actual time created could be like 'Tue Oct 27 2015 19:00:00 GMT-0500 (CDT)',[m
[32m+[m[32m  // i.e. the date-only string is treated as UTC time instead of local time[m
[32m+[m[32m  if ((method === 'toDateString' || method === 'toLocaleDateString') && /^\d{4,}\D\d{2}\D\d{2}$/.test(d)) {[m
[32m+[m[32m    d = d.split(/\D/);[m
[32m+[m[32m    d = new Date(d[0], d[1] - 1, d[2]);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    d = new Date(d);[m
[32m+[m[32m  }[m
[32m+[m[32m  return d[method].apply(d, params);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mwindow.DTWidget = DTWidget;[m
[32m+[m
[32m+[m[32m// A helper function to update the properties of existing filters[m
[32m+[m[32mvar setFilterProps = function(td, props) {[m
[32m+[m[32m  // Update enabled/disabled state[m
[32m+[m[32m  var $input = $(td).find('input').first();[m
[32m+[m[32m  var searchable = $input.data('searchable');[m
[32m+[m[32m  $input.prop('disabled', !searchable || props.disabled);[m
[32m+[m
[32m+[m[32m  // Based on the filter type, set its new values[m
[32m+[m[32m  var type = td.getAttribute('data-type');[m
[32m+[m[32m  if (['factor', 'logical'].includes(type)) {[m
[32m+[m[32m    // Reformat the new dropdown options for use with selectize[m
[32m+[m[32m    var new_vals = props.params.options.map(function(item) {[m
[32m+[m[32m      return { text: item, value: item };[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Find the selectize object[m
[32m+[m[32m    var dropdown = $(td).find('.selectized').eq(0)[0].selectize;[m
[32m+[m
[32m+[m[32m    // Note the current values[m
[32m+[m[32m    var old_vals = dropdown.getValue();[m
[32m+[m
[32m+[m[32m    // Remove the existing values[m
[32m+[m[32m    dropdown.clearOptions();[m
[32m+[m
[32m+[m[32m    // Add the new options[m
[32m+[m[32m    dropdown.addOption(new_vals);[m
[32m+[m
[32m+[m[32m    // Preserve the existing values[m
[32m+[m[32m    dropdown.setValue(old_vals);[m
[32m+[m
[32m+[m[32m  } else if (['number', 'integer', 'date', 'time'].includes(type)) {[m
[32m+[m[32m    // Apply internal scaling to new limits. Updating scale not yet implemented.[m
[32m+[m[32m    var slider = $(td).find('.noUi-target').eq(0);[m
[32m+[m[32m    var scale = Math.pow(10, Math.max(0, +slider.data('scale') || 0));[m
[32m+[m[32m    var new_vals = [props.params.min * scale, props.params.max * scale];[m
[32m+[m
[32m+[m[32m    // Note what the new limits will be just for this filter[m
[32m+[m[32m    var new_lims = new_vals.slice();[m
[32m+[m
[32m+[m[32m    // Determine the current values and limits[m
[32m+[m[32m    var old_vals = slider.val().map(Number);[m
[32m+[m[32m    var old_lims = slider.noUiSlider('options').range;[m
[32m+[m[32m    old_lims = [old_lims.min, old_lims.max];[m
[32m+[m
[32m+[m[32m    // Preserve the current values if filters have been applied; otherwise, apply no filtering[m
[32m+[m[32m    if (old_vals[0] != old_lims[0]) {[m
[32m+[m[32m      new_vals[0] = Math.max(old_vals[0], new_vals[0]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (old_vals[1] != old_lims[1]) {[m
[32m+[m[32m      new_vals[1] = Math.min(old_vals[1], new_vals[1]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Update the endpoints of the slider[m
[32m+[m[32m    slider.noUiSlider({[m
[32m+[m[32m      start: new_vals,[m
[32m+[m[32m      range: {'min': new_lims[0], 'max': new_lims[1]}[m
[32m+[m[32m    }, true);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar transposeArray2D = function(a) {[m
[32m+[m[32m  return a.length === 0 ? a : HTMLWidgets.transposeArray2D(a);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar crosstalkPluginsInstalled = false;[m
[32m+[m
[32m+[m[32mfunction maybeInstallCrosstalkPlugins() {[m
[32m+[m[32m  if (crosstalkPluginsInstalled)[m
[32m+[m[32m    return;[m
[32m+[m[32m  crosstalkPluginsInstalled = true;[m
[32m+[m
[32m+[m[32m  $.fn.dataTable.ext.afnFiltering.push([m
[32m+[m[32m    function(oSettings, aData, iDataIndex) {[m
[32m+[m[32m      var ctfilter = oSettings.nTable.ctfilter;[m
[32m+[m[32m      if (ctfilter && !ctfilter[iDataIndex])[m
[32m+[m[32m        return false;[m
[32m+[m
[32m+[m[32m      var ctselect = oSettings.nTable.ctselect;[m
[32m+[m[32m      if (ctselect && !ctselect[iDataIndex])[m
[32m+[m[32m        return false;[m
[32m+[m
[32m+[m[32m      return true;[m
[32m+[m[32m    }[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mHTMLWidgets.widget({[m
[32m+[m[32m  name: "datatables",[m
[32m+[m[32m  type: "output",[m
[32m+[m[32m  renderOnNullValue: true,[m
[32m+[m[32m  initialize: function(el, width, height) {[m
[32m+[m[32m    // in order that the type=number inputs return a number[m
[32m+[m[32m    $.valHooks.number = {[m
[32m+[m[32m      get: function(el) {[m
[32m+[m[32m        var value = parseFloat(el.value);[m
[32m+[m[32m        return isNaN(value) ? "" : value;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m    $(el).html('&nbsp;');[m
[32m+[m[32m    return {[m
[32m+[m[32m      data: null,[m
[32m+[m[32m      ctfilterHandle: new crosstalk.FilterHandle(),[m
[32m+[m[32m      ctfilterSubscription: null,[m
[32m+[m[32m      ctselectHandle: new crosstalk.SelectionHandle(),[m
[32m+[m[32m      ctselectSubscription: null[m
[32m+[m[32m    };[m
[32m+[m[32m  },[m
[32m+[m[32m  renderValue: function(el, data, instance) {[m
[32m+[m[32m    if (el.offsetWidth === 0 || el.offsetHeight === 0) {[m
[32m+[m[32m      instance.data = data;[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    instance.data = null;[m
[32m+[m[32m    var $el = $(el);[m
[32m+[m[32m    $el.empty();[m
[32m+[m
[32m+[m[32m    if (data === null) {[m
[32m+[m[32m      $el.append('&nbsp;');[m
[32m+[m[32m      // clear previous Shiny inputs (if any)[m
[32m+[m[32m      for (var i in instance.clearInputs) instance.clearInputs[i]();[m
[32m+[m[32m      instance.clearInputs = {};[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var crosstalkOptions = data.crosstalkOptions;[m
[32m+[m[32m    if (!crosstalkOptions) crosstalkOptions = {[m
[32m+[m[32m      'key': null, 'group': null[m
[32m+[m[32m    };[m
[32m+[m[32m    if (crosstalkOptions.group) {[m
[32m+[m[32m      maybeInstallCrosstalkPlugins();[m
[32m+[m[32m      instance.ctfilterHandle.setGroup(crosstalkOptions.group);[m
[32m+[m[32m      instance.ctselectHandle.setGroup(crosstalkOptions.group);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // if we are in the viewer then we always want to fillContainer and[m
[32m+[m[32m    // and autoHideNavigation (unless the user has explicitly set these)[m
[32m+[m[32m    if (window.HTMLWidgets.viewerMode) {[m
[32m+[m[32m      if (!data.hasOwnProperty("fillContainer"))[m
[32m+[m[32m        data.fillContainer = true;[m
[32m+[m[32m      if (!data.hasOwnProperty("autoHideNavigation"))[m
[32m+[m[32m        data.autoHideNavigation = true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // propagate fillContainer to instance (so we have it in resize)[m
[32m+[m[32m    instance.fillContainer = data.fillContainer;[m
[32m+[m
[32m+[m[32m    var cells = data.data;[m
[32m+[m
[32m+[m[32m    if (cells instanceof Array) cells = transposeArray2D(cells);[m
[32m+[m
[32m+[m[32m    $el.append(data.container);[m
[32m+[m[32m    var $table = $el.find('table');[m
[32m+[m[32m    if (data.class) $table.addClass(data.class);[m
[32m+[m[32m    if (data.caption) $table.prepend(data.caption);[m
[32m+[m
[32m+[m[32m    if (!data.selection) data.selection = {[m
[32m+[m[32m      mode: 'none', selected: null, target: 'row', selectable: null[m
[32m+[m[32m    };[m
[32m+[m[32m    if (HTMLWidgets.shinyMode && data.selection.mode !== 'none' &&[m
[32m+[m[32m        data.selection.target === 'row+column') {[m
[32m+[m[32m      if ($table.children('tfoot').length === 0) {[m
[32m+[m[32m        $table.append($('<tfoot>'));[m
[32m+[m[32m        $table.find('thead tr').clone().appendTo($table.find('tfoot'));[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // column filters[m
[32m+[m[32m    var filterRow;[m
[32m+[m[32m    switch (data.filter) {[m
[32m+[m[32m      case 'top':[m
[32m+[m[32m        $table.children('thead').append(data.filterHTML);[m
[32m+[m[32m        filterRow = $table.find('thead tr:last td');[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 'bottom':[m
[32m+[m[32m        if ($table.children('tfoot').length === 0) {[m
[32m+[m[32m          $table.append($('<tfoot>'));[m
[32m+[m[32m        }[m
[32m+[m[32m        $table.children('tfoot').prepend(data.filterHTML);[m
[32m+[m[32m        filterRow = $table.find('tfoot tr:first td');[m
[32m+[m[32m        break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var options = { searchDelay: 1000 };[m
[32m+[m[32m    if (cells !== null) $.extend(options, {[m
[32m+[m[32m      data: cells[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // options for fillContainer[m
[32m+[m[32m    var bootstrapActive = typeof($.fn.popover) != 'undefined';[m
[32m+[m[32m    if (instance.fillContainer) {[m
[32m+[m
[32m+[m[32m      // force scrollX/scrollY and turn off autoWidth[m
[32m+[m[32m      options.scrollX = true;[m
[32m+[m[32m      options.scrollY = "100px"; // can be any value, we'll adjust below[m
[32m+[m
[32m+[m[32m      // if we aren't paginating then move around the info/filter controls[m
[32m+[m[32m      // to save space at the bottom and rephrase the info callback[m
[32m+[m[32m      if (data.options.paging === false) {[m
[32m+[m
[32m+[m[32m        // we know how to do this cleanly for bootstrap, not so much[m
[32m+[m[32m        // for other themes/layouts[m
[32m+[m[32m        if (bootstrapActive) {[m
[32m+[m[32m          options.dom = "<'row'<'col-sm-4'i><'col-sm-8'f>>" +[m
[32m+[m[32m                        "<'row'<'col-sm-12'tr>>";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        options.fnInfoCallback = function(oSettings, iStart, iEnd,[m
[32m+[m[32m                                           iMax, iTotal, sPre) {[m
[32m+[m[32m          return Number(iTotal).toLocaleString() + " records";[m
[32m+[m[32m        };[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // auto hide navigation if requested[m
[32m+[m[32m    // Note, this only works on client-side processing mode as on server-side,[m
[32m+[m[32m    // cells (data.data) is null; In addition, we require the pageLength option[m
[32m+[m[32m    // being provided explicitly to enable this. Despite we may be able to deduce[m
[32m+[m[32m    // the default value of pageLength, it may complicate things so we'd rather[m
[32m+[m[32m    // put this responsiblity to users and warn them on the R side.[m
[32m+[m[32m    if (data.autoHideNavigation === true && data.options.paging !== false) {[m
[32m+[m[32m      // strip all nav if length >= cells[m
[32m+[m[32m      if ((cells instanceof Array) && data.options.pageLength >= cells.length)[m
[32m+[m[32m        options.dom = bootstrapActive ? "<'row'<'col-sm-12'tr>>" : "t";[m
[32m+[m[32m      // alternatively lean things out for flexdashboard mobile portrait[m
[32m+[m[32m      else if (bootstrapActive && window.FlexDashboard && window.FlexDashboard.isMobilePhone())[m
[32m+[m[32m        options.dom = "<'row'<'col-sm-12'f>>" +[m
[32m+[m[32m                      "<'row'<'col-sm-12'tr>>"  +[m
[32m+[m[32m                      "<'row'<'col-sm-12'p>>";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $.extend(true, options, data.options || {});[m
[32m+[m
[32m+[m[32m    var searchCols = options.searchCols;[m
[32m+[m[32m    if (searchCols) {[m
[32m+[m[32m      searchCols = searchCols.map(function(x) {[m
[32m+[m[32m        return x === null ? '' : x.search;[m
[32m+[m[32m      });[m
[32m+[m[32m      // FIXME: this means I don't respect the escapeRegex setting[m
[32m+[m[32m      delete options.searchCols;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // server-side processing?[m
[32m+[m[32m    var server = options.serverSide === true;[m
[32m+[m
[32m+[m[32m    // use the dataSrc function to pre-process JSON data returned from R[m
[32m+[m[32m    var DT_rows_all = [], DT_rows_current = [];[m
[32m+[m[32m    if (server && HTMLWidgets.shinyMode && typeof options.ajax === 'object' &&[m
[32m+[m[32m        /^session\/[\da-z]+\/dataobj/.test(options.ajax.url) && !options.ajax.dataSrc) {[m
[32m+[m[32m      options.ajax.dataSrc = function(json) {[m
[32m+[m[32m        DT_rows_all = $.makeArray(json.DT_rows_all);[m
[32m+[m[32m        DT_rows_current = $.makeArray(json.DT_rows_current);[m
[32m+[m[32m        var data = json.data;[m
[32m+[m[32m        if (!colReorderEnabled()) return data;[m
[32m+[m[32m        var table = $table.DataTable(), order = table.colReorder.order(), flag = true, i, j, row;[m
[32m+[m[32m        for (i = 0; i < order.length; ++i) if (order[i] !== i) flag = false;[m
[32m+[m[32m        if (flag) return data;[m
[32m+[m[32m        for (i = 0; i < data.length; ++i) {[m
[32m+[m[32m          row = data[i].slice();[m
[32m+[m[32m          for (j = 0; j < order.length; ++j) data[i][j] = row[order[j]];[m
[32m+[m[32m        }[m
[32m+[m[32m        return data;[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var thiz = this;[m
[32m+[m[32m    if (instance.fillContainer) $table.on('init.dt', function(e) {[m
[32m+[m[32m      thiz.fillAvailableHeight(el, $(el).innerHeight());[m
[32m+[m[32m    });[m
[32m+[m[32m    // If the page contains serveral datatables and one of which enables colReorder,[m
[32m+[m[32m    // the table.colReorder.order() function will exist but throws error when called.[m
[32m+[m[32m    // So it seems like the only way to know if colReorder is enabled or not is to[m
[32m+[m[32m    // check the options.[m
[32m+[m[32m    var colReorderEnabled = function() { return "colReorder" in options; };[m
[32m+[m[32m    var table = $table.DataTable(options);[m
[32m+[m[32m    $el.data('datatable', table);[m
[32m+[m
[32m+[m[32m    if ('rowGroup' in options) {[m
[32m+[m[32m      // Maintain RowGroup dataSrc when columns are reordered (#1109)[m
[32m+[m[32m      table.on('column-reorder', function(e, settings, details) {[m
[32m+[m[32m        var oldDataSrc = table.rowGroup().dataSrc();[m
[32m+[m[32m        var newDataSrc = details.mapping[oldDataSrc];[m
[32m+[m[32m        table.rowGroup().dataSrc(newDataSrc);[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Unregister previous Crosstalk event subscriptions, if they exist[m
[32m+[m[32m    if (instance.ctfilterSubscription) {[m
[32m+[m[32m      instance.ctfilterHandle.off("change", instance.ctfilterSubscription);[m
[32m+[m[32m      instance.ctfilterSubscription = null;[m
[32m+[m[32m    }[m
[32m+[m[32m    if (instance.ctselectSubscription) {[m
[32m+[m[32m      instance.ctselectHandle.off("change", instance.ctselectSubscription);[m
[32m+[m[32m      instance.ctselectSubscription = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!crosstalkOptions.group) {[m
[32m+[m[32m      $table[0].ctfilter = null;[m
[32m+[m[32m      $table[0].ctselect = null;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      var key = crosstalkOptions.key;[m
[32m+[m[32m      function keysToMatches(keys) {[m
[32m+[m[32m        if (!keys) {[m
[32m+[m[32m          return null;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          var selectedKeys = {};[m
[32m+[m[32m          for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m            selectedKeys[keys[i]] = true;[m
[32m+[m[32m          }[m
[32m+[m[32m          var matches = {};[m
[32m+[m[32m          for (var j = 0; j < key.length; j++) {[m
[32m+[m[32m            if (selectedKeys[key[j]])[m
[32m+[m[32m              matches[j] = true;[m
[32m+[m[32m          }[m
[32m+[m[32m          return matches;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      function applyCrosstalkFilter(e) {[m
[32m+[m[32m        $table[0].ctfilter = keysToMatches(e.value);[m
[32m+[m[32m        table.draw();[m
[32m+[m[32m      }[m
[32m+[m[32m      instance.ctfilterSubscription = instance.ctfilterHandle.on("change", applyCrosstalkFilter);[m
[32m+[m[32m      applyCrosstalkFilter({value: instance.ctfilterHandle.filteredKeys});[m
[32m+[m
[32m+[m[32m      function applyCrosstalkSelection(e) {[m
[32m+[m[32m        if (e.sender !== instance.ctselectHandle) {[m
[32m+[m[32m          table[m
[32m+[m[32m            .rows('.' + selClass, {search: 'applied'})[m
[32m+[m[32m            .nodes()[m
[32m+[m[32m            .to$()[m
[32m+[m[32m            .removeClass(selClass);[m
[32m+[m[32m          if (selectedRows)[m
[32m+[m[32m            changeInput('rows_selected', selectedRows(), void 0, true);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (e.sender !== instance.ctselectHandle && e.value && e.value.length) {[m
[32m+[m[32m          var matches = keysToMatches(e.value);[m
[32m+[m
[32m+[m[32m          // persistent selection with plotly (& leaflet)[m
[32m+[m[32m          var ctOpts = crosstalk.var("plotlyCrosstalkOpts").get() || {};[m
[32m+[m[32m          if (ctOpts.persistent === true) {[m
[32m+[m[32m            var matches = $.extend(matches, $table[0].ctselect);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          $table[0].ctselect = matches;[m
[32m+[m[32m          table.draw();[m
[32m+[m[32m        } else {[m
[32m+[m[32m          if ($table[0].ctselect) {[m
[32m+[m[32m            $table[0].ctselect = null;[m
[32m+[m[32m            table.draw();[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      instance.ctselectSubscription = instance.ctselectHandle.on("change", applyCrosstalkSelection);[m
[32m+[m[32m      // TODO: This next line doesn't seem to work when renderDataTable is used[m
[32m+[m[32m      applyCrosstalkSelection({value: instance.ctselectHandle.value});[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var inArray = function(val, array) {[m
[32m+[m[32m      return $.inArray(val, $.makeArray(array)) > -1;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // search the i-th column[m
[32m+[m[32m    var searchColumn = function(i, value) {[m
[32m+[m[32m      var regex = false, ci = true;[m
[32m+[m[32m      if (options.search) {[m
[32m+[m[32m        regex = options.search.regex,[m
[32m+[m[32m        ci = options.search.caseInsensitive !== false;[m
[32m+[m[32m      }[m
[32m+[m[32m      // need to transpose the column index when colReorder is enabled[m
[32m+[m[32m      if (table.colReorder) i = table.colReorder.transpose(i);[m
[32m+[m[32m      return table.column(i).search(value, regex, !regex, ci);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (data.filter !== 'none') {[m
[32m+[m[32m      if (!data.hasOwnProperty('filterSettings')) data.filterSettings = {};[m
[32m+[m
[32m+[m[32m      filterRow.each(function(i, td) {[m
[32m+[m
[32m+[m[32m        var $td = $(td), type = $td.data('type'), filter;[m
[32m+[m[32m        var $input = $td.children('div').first().children('input');[m
[32m+[m[32m        var disabled = $input.prop('disabled');[m
[32m+[m[32m        var searchable = table.settings()[0].aoColumns[i].bSearchable;[m
[32m+[m[32m        $input.prop('disabled', !searchable || disabled);[m
[32m+[m[32m        $input.data('searchable', searchable); // for updating later[m
[32m+[m[32m        $input.on('input blur', function() {[m
[32m+[m[32m          $input.next('span').toggle(Boolean($input.val()));[m
[32m+[m[32m        });[m
[32m+[m[32m        // Bootstrap sets pointer-events to none and we won't be able to click[m
[32m+[m[32m        // the clear button[m
[32m+[m[32m        $input.next('span').css('pointer-events', 'auto').hide().click(function() {[m
[32m+[m[32m          $(this).hide().prev('input').val('').trigger('input').focus();[m
[32m+[m[32m        });[m
[32m+[m[32m        var searchCol;  // search string for this column[m
[32m+[m[32m        if (searchCols && searchCols[i]) {[m
[32m+[m[32m          searchCol = searchCols[i];[m
[32m+[m[32m          $input.val(searchCol).trigger('input');[m
[32m+[m[32m        }[m
[32m+[m[32m        var $x = $td.children('div').last();[m
[32m+[m
[32m+[m[32m        // remove the overflow: hidden attribute of the scrollHead[m
[32m+[m[32m        // (otherwise the scrolling table body obscures the filters)[m
[32m+[m[32m        // The workaround and the discussion from[m
[32m+[m[32m        // https://github.com/rstudio/DT/issues/554#issuecomment-518007347[m
[32m+[m[32m        // Otherwise the filter selection will not be anchored to the values[m
[32m+[m[32m        // when the columns number is many and scrollX is enabled.[m
[32m+[m[32m        var scrollHead = $(el).find('.dataTables_scrollHead,.dataTables_scrollFoot');[m
[32m+[m[32m        var cssOverflowHead = scrollHead.css('overflow');[m
[32m+[m[32m        var scrollBody = $(el).find('.dataTables_scrollBody');[m
[32m+[m[32m        var cssOverflowBody = scrollBody.css('overflow');[m
[32m+[m[32m        var scrollTable = $(el).find('.dataTables_scroll');[m
[32m+[m[32m        var cssOverflowTable = scrollTable.css('overflow');[m
[32m+[m[32m        if (cssOverflowHead === 'hidden') {[m
[32m+[m[32m          $x.on('show hide', function(e) {[m
[32m+[m[32m            if (e.type === 'show') {[m
[32m+[m[32m              scrollHead.css('overflow', 'visible');[m
[32m+[m[32m              scrollBody.css('overflow', 'visible');[m
[32m+[m[32m              scrollTable.css('overflow-x', 'scroll');[m
[32m+[m[32m            } else {[m
[32m+[m[32m              scrollHead.css('overflow', cssOverflowHead);[m
[32m+[m[32m              scrollBody.css('overflow', cssOverflowBody);[m
[32m+[m[32m              scrollTable.css('overflow-x', cssOverflowTable);[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          $x.css('z-index', 25);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (inArray(type, ['factor', 'logical'])) {[m
[32m+[m[32m          $input.on({[m
[32m+[m[32m            click: function() {[m
[32m+[m[32m              $input.parent().hide(); $x.show().trigger('show'); filter[0].selectize.focus();[m
[32m+[m[32m            },[m
[32m+[m[32m            input: function() {[m
[32m+[m[32m              var v1 = JSON.stringify(filter[0].selectize.getValue()), v2 = $input.val();[m
[32m+[m[32m              if (v1 === '[]') v1 = '';[m
[32m+[m[32m              if (v1 !== v2) filter[0].selectize.setValue(v2 === '' ? [] : JSON.parse(v2));[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          var $input2 = $x.children('select');[m
[32m+[m[32m          filter = $input2.selectize($.extend({[m
[32m+[m[32m            options: $input2.data('options').map(function(v, i) {[m
[32m+[m[32m              return ({text: v, value: v});[m
[32m+[m[32m            }),[m
[32m+[m[32m            plugins: ['remove_button'],[m
[32m+[m[32m            hideSelected: true,[m
[32m+[m[32m            onChange: function(value) {[m
[32m+[m[32m              if (value === null) value = []; // compatibility with jQuery 3.0[m
[32m+[m[32m              $input.val(value.length ? JSON.stringify(value) : '');[m
[32m+[m[32m              if (value.length) $input.trigger('input');[m
[32m+[m[32m              $input.attr('title', $input.val());[m
[32m+[m[32m              if (server) {[m
[32m+[m[32m                searchColumn(i, value.length ? JSON.stringify(value) : '').draw();[m
[32m+[m[32m                return;[m
[32m+[m[32m              }[m
[32m+[m[32m              // turn off filter if nothing selected[m
[32m+[m[32m              $td.data('filter', value.length > 0);[m
[32m+[m[32m              table.draw();  // redraw table, and filters will be applied[m
[32m+[m[32m            }[m
[32m+[m[32m          }, data.filterSettings.select));[m
[32m+[m[32m          filter[0].selectize.on('blur', function() {[m
[32m+[m[32m            $x.hide().trigger('hide'); $input.parent().show(); $input.trigger('blur');[m
[32m+[m[32m          });[m
[32m+[m[32m          filter.next('div').css('margin-bottom', 'auto');[m
[32m+[m[32m        } else if (type === 'character') {[m
[32m+[m[32m          var fun = function() {[m
[32m+[m[32m            searchColumn(i, $input.val()).draw();[m
[32m+[m[32m          };[m
[32m+[m[32m          // throttle searching for server-side processing[m
[32m+[m[32m          var throttledFun = $.fn.dataTable.util.throttle(fun, options.searchDelay);[m
[32m+[m[32m          $input.on('input', function(e, immediate) {[m
[32m+[m[32m            // always bypass throttling when immediate = true (via the updateSearch method)[m
[32m+[m[32m            (immediate || !server) ? fun() : throttledFun();[m
[32m+[m[32m          });[m
[32m+[m[32m        } else if (inArray(type, ['number', 'integer', 'date', 'time'])) {[m
[32m+[m[32m          var $x0 = $x;[m
[32m+[m[32m          $x = $x0.children('div').first();[m
[32m+[m[32m          $x0.css({[m
[32m+[m[32m            'background-color': '#fff',[m
[32m+[m[32m            'border': '1px #ddd solid',[m
[32m+[m[32m            'border-radius': '4px',[m
[32m+[m[32m            'padding': data.vertical ? '35px 20px': '20px 20px 10px 20px'[m
[32m+[m[32m          });[m
[32m+[m[32m          var $spans = $x0.children('span').css({[m
[32m+[m[32m            'margin-top': data.vertical ? '0' : '10px',[m
[32m+[m[32m            'white-space': 'nowrap'[m
[32m+[m[32m          });[m
[32m+[m[32m          var $span1 = $spans.first(), $span2 = $spans.last();[m
[32m+[m[32m          var r1 = +$x.data('min'), r2 = +$x.data('max');[m
[32m+[m[32m          // when the numbers are too small or have many decimal places, the[m
[32m+[m[32m          // slider may have numeric precision problems (#150)[m
[32m+[m[32m          var scale = Math.pow(10, Math.max(0, +$x.data('scale') || 0));[m
[32m+[m[32m          r1 = Math.round(r1 * scale); r2 = Math.round(r2 * scale);[m
[32m+[m[32m          var scaleBack = function(x, scale) {[m
[32m+[m[32m            if (scale === 1) return x;[m
[32m+[m[32m            var d = Math.round(Math.log(scale) / Math.log(10));[m
[32m+[m[32m            // to avoid problems like 3.423/100 -> 0.034230000000000003[m
[32m+[m[32m            return (x / scale).toFixed(d);[m
[32m+[m[32m          };[m
[32m+[m[32m          var slider_min = function() {[m
[32m+[m[32m            return filter.noUiSlider('options').range.min;[m
[32m+[m[32m          };[m
[32m+[m[32m          var slider_max = function() {[m
[32m+[m[32m            return filter.noUiSlider('options').range.max;[m
[32m+[m[32m          };[m
[32m+[m[32m          $input.on({[m
[32m+[m[32m            focus: function() {[m
[32m+[m[32m              $x0.show().trigger('show');[m
[32m+[m[32m              // first, make sure the slider div leaves at least 20px between[m
[32m+[m[32m              // the two (slider value) span's[m
[32m+[m[32m              $x0.width(Math.max(160, $span1.outerWidth() + $span2.outerWidth() + 20));[m
[32m+[m[32m              // then, if the input is really wide or slider is vertical,[m
[32m+[m[32m              // make the slider the same width as the input[m
[32m+[m[32m              if ($x0.outerWidth() < $input.outerWidth() || data.vertical) {[m
[32m+[m[32m                $x0.outerWidth($input.outerWidth());[m
[32m+[m[32m              }[m
[32m+[m[32m              // make sure the slider div does not reach beyond the right margin[m
[32m+[m[32m              if ($(window).width() < $x0.offset().left + $x0.width()) {[m
[32m+[m[32m                $x0.offset({[m
[32m+[m[32m                  'left': $input.offset().left + $input.outerWidth() - $x0.outerWidth()[m
[32m+[m[32m                });[m
[32m+[m[32m              }[m
[32m+[m[32m            },[m
[32m+[m[32m            blur: function() {[m
[32m+[m[32m              $x0.hide().trigger('hide');[m
[32m+[m[32m            },[m
[32m+[m[32m            input: function() {[m
[32m+[m[32m              if ($input.val() === '') filter.val([slider_min(), slider_max()]);[m
[32m+[m[32m            },[m
[32m+[m[32m            change: function() {[m
[32m+[m[32m              var v = $input.val().replace(/\s/g, '');[m
[32m+[m[32m              if (v === '') return;[m
[32m+[m[32m              v = v.split('...');[m
[32m+[m[32m              if (v.length !== 2) {[m
[32m+[m[32m                $input.parent().addClass('has-error');[m
[32m+[m[32m                return;[m
[32m+[m[32m              }[m
[32m+[m[32m              if (v[0] === '') v[0] = slider_min();[m
[32m+[m[32m              if (v[1] === '') v[1] = slider_max();[m
[32m+[m[32m              $input.parent().removeClass('has-error');[m
[32m+[m[32m              // treat date as UTC time at midnight[m
[32m+[m[32m              var strTime = function(x) {[m
[32m+[m[32m                var s = type === 'date' ? 'T00:00:00Z' : '';[m
[32m+[m[32m                var t = new Date(x + s).getTime();[m
[32m+[m[32m                // add 10 minutes to date since it does not hurt the date, and[m
[32m+[m[32m                // it helps avoid the tricky floating point arithmetic problems,[m
[32m+[m[32m                // e.g. sometimes the date may be a few milliseconds earlier[m
[32m+[m[32m                // than the midnight due to precision problems in noUiSlider[m
[32m+[m[32m                return type === 'date' ? t + 3600000 : t;[m
[32m+[m[32m              };[m
[32m+[m[32m              if (inArray(type, ['date', 'time'])) {[m
[32m+[m[32m                v[0] = strTime(v[0]);[m
[32m+[m[32m                v[1] = strTime(v[1]);[m
[32m+[m[32m              }[m
[32m+[m[32m              if (v[0] != slider_min()) v[0] *= scale;[m
[32m+[m[32m              if (v[1] != slider_max()) v[1] *= scale;[m
[32m+[m[32m              filter.val(v);[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          var formatDate = function(d) {[m
[32m+[m[32m            d = scaleBack(d, scale);[m
[32m+[m[32m            if (type === 'number') return d;[m
[32m+[m[32m            if (type === 'integer') return parseInt(d);[m
[32m+[m[32m            var x = new Date(+d);[m
[32m+[m[32m            if (type === 'date') {[m
[32m+[m[32m              var pad0 = function(x) {[m
[32m+[m[32m                return ('0' + x).substr(-2, 2);[m
[32m+[m[32m              };[m
[32m+[m[32m              return x.getUTCFullYear() + '-' + pad0(1 + x.getUTCMonth())[m
[32m+[m[32m                      + '-' + pad0(x.getUTCDate());[m
[32m+[m[32m            } else {[m
[32m+[m[32m              return x.toISOString();[m
[32m+[m[32m            }[m
[32m+[m[32m          };[m
[32m+[m[32m          var opts = type === 'date' ? { step: 60 * 60 * 1000 } :[m
[32m+[m[32m                     type === 'integer' ? { step: 1 } : {};[m
[32m+[m
[32m+[m[32m          opts.orientation = data.vertical ? 'vertical': 'horizontal';[m
[32m+[m[32m          opts.direction = data.vertical ? 'rtl': 'ltr';[m
[32m+[m
[32m+[m[32m          filter = $x.noUiSlider($.extend({[m
[32m+[m[32m            start: [r1, r2],[m
[32m+[m[32m            range: {min: r1, max: r2},[m
[32m+[m[32m            connect: true[m
[32m+[m[32m          }, opts, data.filterSettings.slider));[m
[32m+[m[32m          if (scale > 1) (function() {[m
[32m+[m[32m            var t1 = r1, t2 = r2;[m
[32m+[m[32m            var val = filter.val();[m
[32m+[m[32m            while (val[0] > r1 || val[1] < r2) {[m
[32m+[m[32m              if (val[0] > r1) {[m
[32m+[m[32m                t1 -= val[0] - r1;[m
[32m+[m[32m              }[m
[32m+[m[32m              if (val[1] < r2) {[m
[32m+[m[32m                t2 += r2 - val[1];[m
[32m+[m[32m              }[m
[32m+[m[32m              filter = $x.noUiSlider($.extend({[m
[32m+[m[32m                start: [t1, t2],[m
[32m+[m[32m                range: {min: t1, max: t2},[m
[32m+[m[32m                connect: true[m
[32m+[m[32m              }, opts, data.filterSettings.slider), true);[m
[32m+[m[32m              val = filter.val();[m
[32m+[m[32m            }[m
[32m+[m[32m            r1  = t1; r2 = t2;[m
[32m+[m[32m          })();[m
[32m+[m[32m          // format with active column renderer, if defined[m
[32m+[m[32m          var colDef = data.options.columnDefs.find(function(def) {[m
[32m+[m[32m            return (def.targets === i || inArray(i, def.targets)) && 'render' in def;[m
[32m+[m[32m          });[m
[32m+[m[32m          var updateSliderText = function(v1, v2) {[m
[32m+[m[32m            // we only know how to use function renderers[m
[32m+[m[32m            if (colDef && typeof colDef.render === 'function') {[m
[32m+[m[32m              var restore = function(v) {[m
[32m+[m[32m                v = scaleBack(v, scale);[m
[32m+[m[32m                return inArray(type, ['date', 'time']) ? new Date(+v) : v;[m
[32m+[m[32m              }[m
[32m+[m[32m              $span1.text(colDef.render(restore(v1), 'display'));[m
[32m+[m[32m              $span2.text(colDef.render(restore(v2), 'display'));[m
[32m+[m[32m            } else {[m
[32m+[m[32m              $span1.text(formatDate(v1));[m
[32m+[m[32m              $span2.text(formatDate(v2));[m
[32m+[m[32m            }[m
[32m+[m[32m          };[m
[32m+[m[32m          updateSliderText(r1, r2);[m
[32m+[m[32m          var updateSlider = function(e) {[m
[32m+[m[32m            var val = filter.val();[m
[32m+[m[32m            // turn off filter if in full range[m
[32m+[m[32m            $td.data('filter', val[0] > slider_min() || val[1] < slider_max());[m
[32m+[m[32m            var v1 = formatDate(val[0]), v2 = formatDate(val[1]), ival;[m
[32m+[m[32m            if ($td.data('filter')) {[m
[32m+[m[32m              ival = v1 + ' ... ' + v2;[m
[32m+[m[32m              $input.attr('title', ival).val(ival).trigger('input');[m
[32m+[m[32m            } else {[m
[32m+[m[32m              $input.attr('title', '').val('');[m
[32m+[m[32m            }[m
[32m+[m[32m            updateSliderText(val[0], val[1]);[m
[32m+[m[32m            if (e.type === 'slide') return;  // no searching when sliding only[m
[32m+[m[32m            if (server) {[m
[32m+[m[32m              searchColumn(i, $td.data('filter') ? ival : '').draw();[m
[32m+[m[32m              return;[m
[32m+[m[32m            }[m
[32m+[m[32m            table.draw();[m
[32m+[m[32m          };[m
[32m+[m[32m          filter.on({[m
[32m+[m[32m            set: updateSlider,[m
[32m+[m[32m            slide: updateSlider[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // server-side processing will be handled by R (or whatever server[m
[32m+[m[32m        // language you use); the following code is only needed for client-side[m
[32m+[m[32m        // processing[m
[32m+[m[32m        if (server) {[m
[32m+[m[32m          // if a search string has been pre-set, search now[m
[32m+[m[32m          if (searchCol) $input.trigger('input').trigger('change');[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var customFilter = function(settings, data, dataIndex) {[m
[32m+[m[32m          // there is no way to attach a search function to a specific table,[m
[32m+[m[32m          // and we need to make sure a global search function is not applied to[m
[32m+[m[32m          // all tables (i.e. a range filter in a previous table should not be[m
[32m+[m[32m          // applied to the current table); we use the settings object to[m
[32m+[m[32m          // determine if we want to perform searching on the current table,[m
[32m+[m[32m          // since settings.sTableId will be different to different tables[m
[32m+[m[32m          if (table.settings()[0] !== settings) return true;[m
[32m+[m[32m          // no filter on this column or no need to filter this column[m
[32m+[m[32m          if (typeof filter === 'undefined' || !$td.data('filter')) return true;[m
[32m+[m
[32m+[m[32m          var r = filter.val(), v, r0, r1;[m
[32m+[m[32m          var i_data = function(i) {[m
[32m+[m[32m            if (!colReorderEnabled()) return i;[m
[32m+[m[32m            var order = table.colReorder.order(), k;[m
[32m+[m[32m            for (k = 0; k < order.length; ++k) if (order[k] === i) return k;[m
[32m+[m[32m            return i; // in theory it will never be here...[m
[32m+[m[32m          }[m
[32m+[m[32m          v = data[i_data(i)];[m
[32m+[m[32m          if (type === 'number' || type === 'integer') {[m
[32m+[m[32m            v = parseFloat(v);[m
[32m+[m[32m            // how to handle NaN? currently exclude these rows[m
[32m+[m[32m            if (isNaN(v)) return(false);[m
[32m+[m[32m            r0 = parseFloat(scaleBack(r[0], scale))[m
[32m+[m[32m            r1 = parseFloat(scaleBack(r[1], scale));[m
[32m+[m[32m            if (v >= r0 && v <= r1) return true;[m
[32m+[m[32m          } else if (type === 'date' || type === 'time') {[m
[32m+[m[32m            v = new Date(v);[m
[32m+[m[32m            r0 = new Date(r[0] / scale); r1 = new Date(r[1] / scale);[m
[32m+[m[32m            if (v >= r0 && v <= r1) return true;[m
[32m+[m[32m          } else if (type === 'factor') {[m
[32m+[m[32m            if (r.length === 0 || inArray(v, r)) return true;[m
[32m+[m[32m          } else if (type === 'logical') {[m
[32m+[m[32m            if (r.length === 0) return true;[m
[32m+[m[32m            if (inArray(v === '' ? 'na' : v, r)) return true;[m
[32m+[m[32m          }[m
[32m+[m[32m          return false;[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        $.fn.dataTable.ext.search.push(customFilter);[m
[32m+[m
[32m+[m[32m        // search for the preset search strings if it is non-empty[m
[32m+[m[32m        if (searchCol) $input.trigger('input').trigger('change');[m
[32m+[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // highlight search keywords[m
[32m+[m[32m    var highlight = function() {[m
[32m+[m[32m      var body = $(table.table().body());[m
[32m+[m[32m      // removing the old highlighting first[m
[32m+[m[32m      body.unhighlight();[m
[32m+[m
[32m+[m[32m      // don't highlight the "not found" row, so we get the rows using the api[m
[32m+[m[32m      if (table.rows({ filter: 'applied' }).data().length === 0) return;[m
[32m+[m[32m      // highlight global search keywords[m
[32m+[m[32m      body.highlight($.trim(table.search()).split(/\s+/));[m
[32m+[m[32m      // then highlight keywords from individual column filters[m
[32m+[m[32m      if (filterRow) filterRow.each(function(i, td) {[m
[32m+[m[32m        var $td = $(td), type = $td.data('type');[m
[32m+[m[32m        if (type !== 'character') return;[m
[32m+[m[32m        var $input = $td.children('div').first().children('input');[m
[32m+[m[32m        var column = table.column(i).nodes().to$(),[m
[32m+[m[32m            val = $.trim($input.val());[m
[32m+[m[32m        if (type !== 'character' || val === '') return;[m
[32m+[m[32m        column.highlight(val.split(/\s+/));[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (options.searchHighlight) {[m
[32m+[m[32m      table[m
[32m+[m[32m      .on('draw.dt.dth column-visibility.dt.dth column-reorder.dt.dth', highlight)[m
[32m+[m[32m      .on('destroy', function() {[m
[32m+[m[32m        // remove event handler[m
[32m+[m[32m        table.off('draw.dt.dth column-visibility.dt.dth column-reorder.dt.dth');[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // Set the option for escaping regex characters in our search string.  This will be used[m
[32m+[m[32m      // for all future matching.[m
[32m+[m[32m      jQuery.fn.highlight.options.escapeRegex = (!options.search || !options.search.regex);[m
[32m+[m
[32m+[m[32m      // initial highlight for state saved conditions and initial states[m
[32m+[m[32m      highlight();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // run the callback function on the table instance[m
[32m+[m[32m    if (typeof data.callback === 'function') data.callback(table);[m
[32m+[m
[32m+[m[32m    // double click to edit the cell, row, column, or all cells[m
[32m+[m[32m    if (data.editable) table.on('dblclick.dt', 'tbody td', function(e) {[m
[32m+[m[32m      // only bring up the editor when the cell itself is dbclicked, and ignore[m
[32m+[m[32m      // other dbclick events bubbled up (e.g. from the <input>)[m
[32m+[m[32m      if (e.target !== this) return;[m
[32m+[m[32m      var target = [], immediate = false;[m
[32m+[m[32m      switch (data.editable.target) {[m
[32m+[m[32m        case 'cell':[m
[32m+[m[32m          target = [this];[m
[32m+[m[32m          immediate = true;  // edit will take effect immediately[m
[32m+[m[32m          break;[m
[32m+[m[32m        case 'row':[m
[32m+[m[32m          target = table.cells(table.cell(this).index().row, '*').nodes();[m
[32m+[m[32m          break;[m
[32m+[m[32m        case 'column':[m
[32m+[m[32m          target = table.cells('*', table.cell(this).index().column).nodes();[m
[32m+[m[32m          break;[m
[32m+[m[32m        case 'all':[m
[32m+[m[32m          target = table.cells().nodes();[m
[32m+[m[32m          break;[m
[32m+[m[32m        default:[m
[32m+[m[32m          throw 'The editable parameter must be "cell", "row", "column", or "all"';[m
[32m+[m[32m      }[m
[32m+[m[32m      var disableCols = data.editable.disable ? data.editable.disable.columns : null;[m
[32m+[m[32m      var numericCols = data.editable.numeric;[m
[32m+[m[32m      var areaCols = data.editable.area;[m
[32m+[m[32m      var dateCols = data.editable.date;[m
[32m+[m[32m      for (var i = 0; i < target.length; i++) {[m
[32m+[m[32m        (function(cell, current) {[m
[32m+[m[32m          var $cell = $(cell), html = $cell.html();[m
[32m+[m[32m          var _cell = table.cell(cell), value = _cell.data(), index = _cell.index().column;[m
[32m+[m[32m          var $input;[m
[32m+[m[32m          if (inArray(index, numericCols)) {[m
[32m+[m[32m            $input = $('<input type="number">');[m
[32m+[m[32m          } else if (inArray(index, areaCols)) {[m
[32m+[m[32m            $input = $('<textarea></textarea>');[m
[32m+[m[32m          } else if (inArray(index, dateCols)) {[m
[32m+[m[32m            $input = $('<input type="date">');[m
[32m+[m[32m          } else {[m
[32m+[m[32m            $input = $('<input type="text">');[m
[32m+[m[32m          }[m
[32m+[m[32m          if (!immediate) {[m
[32m+[m[32m            $cell.data('input', $input).data('html', html);[m
[32m+[m[32m            $input.attr('title', 'Hit Ctrl+Enter to finish editing, or Esc to cancel');[m
[32m+[m[32m          }[m
[32m+[m[32m          $input.val(value);[m
[32m+[m[32m          if (inArray(index, disableCols)) {[m
[32m+[m[32m            $input.attr('readonly', '').css('filter', 'invert(25%)');[m
[32m+[m[32m          }[m
[32m+[m[32m          $cell.empty().append($input);[m
[32m+[m[32m          if (cell === current) $input.focus();[m
[32m+[m[32m          $input.css('width', '100%');[m
[32m+[m
[32m+[m[32m          if (immediate) $input.on('blur', function(e) {[m
[32m+[m[32m            var valueNew = $input.val();[m
[32m+[m[32m            if (valueNew !== value) {[m
[32m+[m[32m              _cell.data(valueNew);[m
[32m+[m[32m              if (HTMLWidgets.shinyMode) {[m
[32m+[m[32m                changeInput('cell_edit', [cellInfo(cell)], 'DT.cellInfo', null, {priority: 'event'});[m
[32m+[m[32m              }[m
[32m+[m[32m              // for server-side processing, users have to call replaceData() to update the table[m
[32m+[m[32m              if (!server) table.draw(false);[m
[32m+[m[32m            } else {[m
[32m+[m[32m              $cell.html(html);[m
[32m+[m[32m            }[m
[32m+[m[32m          }).on('keyup', function(e) {[m
[32m+[m[32m            // hit Escape to cancel editing[m
[32m+[m[32m            if (e.keyCode === 27) $input.trigger('blur');[m
[32m+[m[32m          });[m
[32m+[m
[32m+[m[32m          // bulk edit (row, column, or all)[m
[32m+[m[32m          if (!immediate) $input.on('keyup', function(e) {[m
[32m+[m[32m            var removeInput = function($cell, restore) {[m
[32m+[m[32m              $cell.data('input').remove();[m
[32m+[m[32m              if (restore) $cell.html($cell.data('html'));[m
[32m+[m[32m            }[m
[32m+[m[32m            if (e.keyCode === 27) {[m
[32m+[m[32m              for (var i = 0; i < target.length; i++) {[m
[32m+[m[32m                removeInput($(target[i]), true);[m
[32m+[m[32m              }[m
[32m+[m[32m            } else if (e.keyCode === 13 && e.ctrlKey) {[m
[32m+[m[32m              // Ctrl + Enter[m
[32m+[m[32m              var cell, $cell, _cell, cellData = [];[m
[32m+[m[32m              for (var i = 0; i < target.length; i++) {[m
[32m+[m[32m                cell = target[i]; $cell = $(cell); _cell = table.cell(cell);[m
[32m+[m[32m                _cell.data($cell.data('input').val());[m
[32m+[m[32m                HTMLWidgets.shinyMode && cellData.push(cellInfo(cell));[m
[32m+[m[32m                removeInput($cell, false);[m
[32m+[m[32m              }[m
[32m+[m[32m              if (HTMLWidgets.shinyMode) {[m
[32m+[m[32m                changeInput('cell_edit', cellData, 'DT.cellInfo', null, {priority: "event"});[m
[32m+[m[32m              }[m
[32m+[m[32m              if (!server) table.draw(false);[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m        })(target[i], this);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // interaction with shiny[m
[32m+[m[32m    if (!HTMLWidgets.shinyMode && !crosstalkOptions.group) return;[m
[32m+[m
[32m+[m[32m    var methods = {};[m
[32m+[m[32m    var shinyData = {};[m
[32m+[m
[32m+[m[32m    methods.updateCaption = function(caption) {[m
[32m+[m[32m      if (!caption) return;[m
[32m+[m[32m      $table.children('caption').replaceWith(caption);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // register clear functions to remove input values when the table is removed[m
[32m+[m[32m    instance.clearInputs = {};[m
[32m+[m
[32m+[m[32m    var changeInput = function(id, value, type, noCrosstalk, opts) {[m
[32m+[m[32m      var event = id;[m
[32m+[m[32m      id = el.id + '_' + id;[m
[32m+[m[32m      if (type) id = id + ':' + type;[m
[32m+[m[32m      // do not update if the new value is the same as old value[m
[32m+[m[32m      if (event !== 'cell_edit' && !/_clicked$/.test(event) && shinyData.hasOwnProperty(id) && shinyData[id] === JSON.stringify(value))[m
[32m+[m[32m        return;[m
[32m+[m[32m      shinyData[id] = JSON.stringify(value);[m
[32m+[m[32m      if (HTMLWidgets.shinyMode && Shiny.setInputValue) {[m
[32m+[m[32m        Shiny.setInputValue(id, value, opts);[m
[32m+[m[32m        if (!instance.clearInputs[id]) instance.clearInputs[id] = function() {[m
[32m+[m[32m          Shiny.setInputValue(id, null);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // HACK[m
[32m+[m[32m      if (event === "rows_selected" && !noCrosstalk) {[m
[32m+[m[32m        if (crosstalkOptions.group) {[m
[32m+[m[32m          var keys = crosstalkOptions.key;[m
[32m+[m[32m          var selectedKeys = null;[m
[32m+[m[32m          if (value) {[m
[32m+[m[32m            selectedKeys = [];[m
[32m+[m[32m            for (var i = 0; i < value.length; i++) {[m
[32m+[m[32m              // The value array's contents use 1-based row numbers, so we must[m
[32m+[m[32m              // convert to 0-based before indexing into the keys array.[m
[32m+[m[32m              selectedKeys.push(keys[value[i] - 1]);[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m          instance.ctselectHandle.set(selectedKeys);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var addOne = function(x) {[m
[32m+[m[32m      return x.map(function(i) { return 1 + i; });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var unique = function(x) {[m
[32m+[m[32m      var ux = [];[m
[32m+[m[32m      $.each(x, function(i, el){[m
[32m+[m[32m        if ($.inArray(el, ux) === -1) ux.push(el);[m
[32m+[m[32m      });[m
[32m+[m[32m      return ux;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // change the row index of a cell[m
[32m+[m[32m    var tweakCellIndex = function(cell) {[m
[32m+[m[32m      var info = cell.index();[m
[32m+[m[32m      // some cell may not be valid. e.g, #759[m
[32m+[m[32m      // when using the RowGroup extension, datatables will[m
[32m+[m[32m      // generate the row label and the cells are not part of[m
[32m+[m[32m      // the data thus contain no row/col info[m
[32m+[m[32m      if (info === undefined)[m
[32m+[m[32m        return {row: null, col: null};[m
[32m+[m[32m      if (server) {[m
[32m+[m[32m        info.row = DT_rows_current[info.row];[m
[32m+[m[32m      } else {[m
[32m+[m[32m        info.row += 1;[m
[32m+[m[32m      }[m
[32m+[m[32m      return {row: info.row, col: info.column};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var cleanSelectedValues = function() {[m
[32m+[m[32m      changeInput('rows_selected', []);[m
[32m+[m[32m      changeInput('columns_selected', []);[m
[32m+[m[32m      changeInput('cells_selected', transposeArray2D([]), 'shiny.matrix');[m
[32m+[m[32m    }[m
[32m+[m[32m    // #828 we should clean the selection on the server-side when the table reloads[m
[32m+[m[32m    cleanSelectedValues();[m
[32m+[m
[32m+[m[32m    // a flag to indicates if select extension is initialized or not[m
[32m+[m[32m    var flagSelectExt = table.settings()[0]._select !== undefined;[m
[32m+[m[32m    // the Select extension should only be used in the client mode and[m
[32m+[m[32m    // when the selection.mode is set to none[m
[32m+[m[32m    if (data.selection.mode === 'none' && !server && flagSelectExt) {[m
[32m+[m[32m      var updateRowsSelected = function() {[m
[32m+[m[32m        var rows = table.rows({selected: true});[m
[32m+[m[32m        var selected = [];[m
[32m+[m[32m        $.each(rows.indexes().toArray(), function(i, v) {[m
[32m+[m[32m          selected.push(v + 1);[m
[32m+[m[32m        });[m
[32m+[m[32m        changeInput('rows_selected', selected);[m
[32m+[m[32m      }[m
[32m+[m[32m      var updateColsSelected = function() {[m
[32m+[m[32m        var columns = table.columns({selected: true});[m
[32m+[m[32m        changeInput('columns_selected', columns.indexes().toArray());[m
[32m+[m[32m      }[m
[32m+[m[32m      var updateCellsSelected = function() {[m
[32m+[m[32m        var cells = table.cells({selected: true});[m
[32m+[m[32m        var selected = [];[m
[32m+[m[32m        cells.every(function() {[m
[32m+[m[32m          var row = this.index().row;[m
[32m+[m[32m          var col = this.index().column;[m
[32m+[m[32m          selected = selected.concat([[row + 1, col]]);[m
[32m+[m[32m        });[m
[32m+[m[32m        changeInput('cells_selected', transposeArray2D(selected), 'shiny.matrix');[m
[32m+[m[32m      }[m
[32m+[m[32m      table.on('select deselect', function(e, dt, type, indexes) {[m
[32m+[m[32m        updateRowsSelected();[m
[32m+[m[32m        updateColsSelected();[m
[32m+[m[32m        updateCellsSelected();[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var selMode = data.selection.mode, selTarget = data.selection.target;[m
[32m+[m[32m    var selDisable = data.selection.selectable === false;[m
[32m+[m[32m    if (inArray(selMode, ['single', 'multiple'])) {[m
[32m+[m[32m      var selClass = inArray(data.style, ['bootstrap', 'bootstrap4']) ? 'active' : 'selected';[m
[32m+[m[32m      // selected1: row indices; selected2: column indices[m
[32m+[m[32m      var initSel = function(x) {[m
[32m+[m[32m        if (x === null || typeof x === 'boolean' || selTarget === 'cell') {[m
[32m+[m[32m          return {rows: [], cols: []};[m
[32m+[m[32m        } else if (selTarget === 'row') {[m
[32m+[m[32m          return {rows: $.makeArray(x), cols: []};[m
[32m+[m[32m        } else if (selTarget === 'column') {[m
[32m+[m[32m          return {rows: [], cols: $.makeArray(x)};[m
[32m+[m[32m        } else if (selTarget === 'row+column') {[m
[32m+[m[32m          return {rows: $.makeArray(x.rows), cols: $.makeArray(x.cols)};[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      var selected = data.selection.selected;[m
[32m+[m[32m      var selected1 = initSel(selected).rows, selected2 = initSel(selected).cols;[m
[32m+[m[32m      // selectable should contain either all positive or all non-positive values, not both[m
[32m+[m[32m      // positive values indicate "selectable" while non-positive values means "nonselectable"[m
[32m+[m[32m      // the assertion is performed on R side. (only column indicides could be zero which indicates[m
[32m+[m[32m      // the row name)[m
[32m+[m[32m      var selectable = data.selection.selectable;[m
[32m+[m[32m      var selectable1 = initSel(selectable).rows, selectable2 = initSel(selectable).cols;[m
[32m+[m
[32m+[m[32m      // After users reorder the rows or filter the table, we cannot use the table index[m
[32m+[m[32m      // directly. Instead, we need this function to find out the rows between the two clicks.[m
[32m+[m[32m      // If user filter the table again between the start click and the end click, the behavior[m
[32m+[m[32m      // would be undefined, but it should not be a problem.[m
[32m+[m[32m      var shiftSelRowsIndex = function(start, end) {[m
[32m+[m[32m        var indexes = server ? DT_rows_all : table.rows({ search: 'applied' }).indexes().toArray();[m
[32m+[m[32m        start = indexes.indexOf(start); end = indexes.indexOf(end);[m
[32m+[m[32m        // if start is larger than end, we need to swap[m
[32m+[m[32m        if (start > end) {[m
[32m+[m[32m          var tmp = end; end = start; start = tmp;[m
[32m+[m[32m        }[m
[32m+[m[32m        return indexes.slice(start, end + 1);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var serverRowIndex = function(clientRowIndex) {[m
[32m+[m[32m        return server ? DT_rows_current[clientRowIndex] : clientRowIndex + 1;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // row, column, or cell selection[m
[32m+[m[32m      var lastClickedRow;[m
[32m+[m[32m      if (inArray(selTarget, ['row', 'row+column'])) {[m
[32m+[m[32m        // Get the current selected rows. It will also[m
[32m+[m[32m        // update the selected1's value based on the current row selection state[m
[32m+[m[32m        // Note we can't put this function inside selectRows() directly,[m
[32m+[m[32m        // the reason is method.selectRows() will override selected1's value but this[m
[32m+[m[32m        // function will add rows to selected1 (keep the existing selection), which is[m
[32m+[m[32m        // inconsistent with column and cell selection.[m
[32m+[m[32m        var selectedRows = function() {[m
[32m+[m[32m          var rows = table.rows('.' + selClass);[m
[32m+[m[32m          var idx = rows.indexes().toArray();[m
[32m+[m[32m          if (!server) {[m
[32m+[m[32m            selected1 = addOne(idx);[m
[32m+[m[32m            return selected1;[m
[32m+[m[32m          }[m
[32m+[m[32m          idx = idx.map(function(i) {[m
[32m+[m[32m            return DT_rows_current[i];[m
[32m+[m[32m          });[m
[32m+[m[32m          selected1 = selMode === 'multiple' ? unique(selected1.concat(idx)) : idx;[m
[32m+[m[32m          return selected1;[m
[32m+[m[32m        }[m
[32m+[m[32m        // Change selected1's value based on selectable1, then refresh the row state[m
[32m+[m[32m        var onlyKeepSelectableRows = function() {[m
[32m+[m[32m          if (selDisable) { // users can't select; useful when only want backend select[m
[32m+[m[32m            selected1 = [];[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (selectable1.length === 0) return;[m
[32m+[m[32m          var nonselectable = selectable1[0] <= 0;[m
[32m+[m[32m          if (nonselectable) {[m
[32m+[m[32m            // should make selectable1 positive[m
[32m+[m[32m            selected1 = $(selected1).not(selectable1.map(function(i) { return -i; })).get();[m
[32m+[m[32m          } else {[m
[32m+[m[32m            selected1 = $(selected1).filter(selectable1).get();[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        // Change selected1's value based on selectable1, then[m
[32m+[m[32m        // refresh the row selection state according to values in selected1[m
[32m+[m[32m        var selectRows = function(ignoreSelectable) {[m
[32m+[m[32m          if (!ignoreSelectable) onlyKeepSelectableRows();[m
[32m+[m[32m          table.$('tr.' + selClass).removeClass(selClass);[m
[32m+[m[32m          if (selected1.length === 0) return;[m
[32m+[m[32m          if (server) {[m
[32m+[m[32m            table.rows({page: 'current'}).every(function() {[m
[32m+[m[32m              if (inArray(DT_rows_current[this.index()], selected1)) {[m
[32m+[m[32m                $(this.node()).addClass(selClass);[m
[32m+[m[32m              }[m
[32m+[m[32m            });[m
[32m+[m[32m          } else {[m
[32m+[m[32m            var selected0 = selected1.map(function(i) { return i - 1; });[m
[32m+[m[32m            $(table.rows(selected0).nodes()).addClass(selClass);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        table.on('mousedown.dt', 'tbody tr', function(e) {[m
[32m+[m[32m          var $this = $(this), thisRow = table.row(this);[m
[32m+[m[32m          if (selMode === 'multiple') {[m
[32m+[m[32m            if (e.shiftKey && lastClickedRow !== undefined) {[m
[32m+[m[32m              // select or de-select depends on the last clicked row's status[m
[32m+[m[32m              var flagSel = !$this.hasClass(selClass);[m
[32m+[m[32m              var crtClickedRow = serverRowIndex(thisRow.index());[m
[32m+[m[32m              if (server) {[m
[32m+[m[32m                var rowsIndex = shiftSelRowsIndex(lastClickedRow, crtClickedRow);[m
[32m+[m[32m                // update current page's selClass[m
[32m+[m[32m                rowsIndex.map(function(i) {[m
[32m+[m[32m                  var rowIndex = DT_rows_current.indexOf(i);[m
[32m+[m[32m                  if (rowIndex >= 0) {[m
[32m+[m[32m                    var row = table.row(rowIndex).nodes().to$();[m
[32m+[m[32m                    var flagRowSel = !row.hasClass(selClass);[m
[32m+[m[32m                    if (flagSel === flagRowSel) row.toggleClass(selClass);[m
[32m+[m[32m                  }[m
[32m+[m[32m                });[m
[32m+[m[32m                // update selected1[m
[32m+[m[32m                if (flagSel) {[m
[32m+[m[32m                  selected1 = unique(selected1.concat(rowsIndex));[m
[32m+[m[32m                } else {[m
[32m+[m[32m                  selected1 = selected1.filter(function(index) {[m
[32m+[m[32m                    return !inArray(index, rowsIndex);[m
[32m+[m[32m                  });[m
[32m+[m[32m                }[m
[32m+[m[32m              } else {[m
[32m+[m[32m                // js starts from 0[m
[32m+[m[32m                shiftSelRowsIndex(lastClickedRow - 1, crtClickedRow - 1).map(function(value) {[m
[32m+[m[32m                  var row = table.row(value).nodes().to$();[m
[32m+[m[32m                  var flagRowSel = !row.hasClass(selClass);[m
[32m+[m[32m                  if (flagSel === flagRowSel) row.toggleClass(selClass);[m
[32m+[m[32m                });[m
[32m+[m[32m              }[m
[32m+[m[32m              e.preventDefault();[m
[32m+[m[32m            } else {[m
[32m+[m[32m              $this.toggleClass(selClass);[m
[32m+[m[32m            }[m
[32m+[m[32m          } else {[m
[32m+[m[32m            if ($this.hasClass(selClass)) {[m
[32m+[m[32m              $this.removeClass(selClass);[m
[32m+[m[32m            } else {[m
[32m+[m[32m              table.$('tr.' + selClass).removeClass(selClass);[m
[32m+[m[32m              $this.addClass(selClass);[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m          if (server && !$this.hasClass(selClass)) {[m
[32m+[m[32m            var id = DT_rows_current[thisRow.index()];[m
[32m+[m[32m            // remove id from selected1 since its class .selected has been removed[m
[32m+[m[32m            if (inArray(id, selected1)) selected1.splice($.inArray(id, selected1), 1);[m
[32m+[m[32m          }[m
[32m+[m[32m          selectedRows(); // update selected1's value based on selClass[m
[32m+[m[32m          selectRows(false); // only keep the selectable rows[m
[32m+[m[32m          changeInput('rows_selected', selected1);[m
[32m+[m[32m          changeInput('row_last_clicked', serverRowIndex(thisRow.index()), null, null, {priority: 'event'});[m
[32m+[m[32m          lastClickedRow = serverRowIndex(thisRow.index());[m
[32m+[m[32m        });[m
[32m+[m[32m        selectRows(false);  // in case users have specified pre-selected rows[m
[32m+[m[32m        // restore selected rows after the table is redrawn (e.g. sort/search/page);[m
[32m+[m[32m        // client-side tables will preserve the selections automatically; for[m
[32m+[m[32m        // server-side tables, we have to *real* row indices are in `selected1`[m
[32m+[m[32m        changeInput('rows_selected', selected1);[m
[32m+[m[32m        if (server) table.on('draw.dt', function(e) { selectRows(false); });[m
[32m+[m[32m        methods.selectRows = function(selected, ignoreSelectable) {[m
[32m+[m[32m          selected1 = $.makeArray(selected);[m
[32m+[m[32m          selectRows(ignoreSelectable);[m
[32m+[m[32m          changeInput('rows_selected', selected1);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (inArray(selTarget, ['column', 'row+column'])) {[m
[32m+[m[32m        if (selTarget === 'row+column') {[m
[32m+[m[32m          $(table.columns().footer()).css('cursor', 'pointer');[m
[32m+[m[32m        }[m
[32m+[m[32m        // update selected2's value based on selectable2[m
[32m+[m[32m        var onlyKeepSelectableCols = function() {[m
[32m+[m[32m          if (selDisable) { // users can't select; useful when only want backend select[m
[32m+[m[32m            selected2 = [];[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (selectable2.length === 0) return;[m
[32m+[m[32m          var nonselectable = selectable2[0] <= 0;[m
[32m+[m[32m          if (nonselectable) {[m
[32m+[m[32m            // need to make selectable2 positive[m
[32m+[m[32m            selected2 = $(selected2).not(selectable2.map(function(i) { return -i; })).get();[m
[32m+[m[32m          } else {[m
[32m+[m[32m            selected2 = $(selected2).filter(selectable2).get();[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        // update selected2 and then[m
[32m+[m[32m        // refresh the col selection state according to values in selected2[m
[32m+[m[32m        var selectCols = function(ignoreSelectable) {[m
[32m+[m[32m          if (!ignoreSelectable) onlyKeepSelectableCols();[m
[32m+[m[32m          // if selected2 is not a valide index (e.g., larger than the column number)[m
[32m+[m[32m          // table.columns(selected2) will fail and result in a blank table[m
[32m+[m[32m          // this is different from the table.rows(), where the out-of-range indexes[m
[32m+[m[32m          // doesn't affect at all[m
[32m+[m[32m          selected2 = $(selected2).filter(table.columns().indexes()).get();[m
[32m+[m[32m          table.columns().nodes().flatten().to$().removeClass(selClass);[m
[32m+[m[32m          if (selected2.length > 0)[m
[32m+[m[32m            table.columns(selected2).nodes().flatten().to$().addClass(selClass);[m
[32m+[m[32m        }[m
[32m+[m[32m        var callback = function() {[m
[32m+[m[32m          var colIdx = selTarget === 'column' ? table.cell(this).index().column :[m
[32m+[m[32m              $.inArray(this, table.columns().footer()),[m
[32m+[m[32m              thisCol = $(table.column(colIdx).nodes());[m
[32m+[m[32m          if (colIdx === -1) return;[m
[32m+[m[32m          if (thisCol.hasClass(selClass)) {[m
[32m+[m[32m            thisCol.removeClass(selClass);[m
[32m+[m[32m            selected2.splice($.inArray(colIdx, selected2), 1);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            if (selMode === 'single') $(table.cells().nodes()).removeClass(selClass);[m
[32m+[m[32m            thisCol.addClass(selClass);[m
[32m+[m[32m            selected2 = selMode === 'single' ? [colIdx] : unique(selected2.concat([colIdx]));[m
[32m+[m[32m          }[m
[32m+[m[32m          selectCols(false); // update selected2 based on selectable[m
[32m+[m[32m          changeInput('columns_selected', selected2);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (selTarget === 'column') {[m
[32m+[m[32m          $(table.table().body()).on('click.dt', 'td', callback);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $(table.table().footer()).on('click.dt', 'tr th', callback);[m
[32m+[m[32m        }[m
[32m+[m[32m        selectCols(false);  // in case users have specified pre-selected columns[m
[32m+[m[32m        changeInput('columns_selected', selected2);[m
[32m+[m[32m        if (server) table.on('draw.dt', function(e) { selectCols(false); });[m
[32m+[m[32m        methods.selectColumns = function(selected, ignoreSelectable) {[m
[32m+[m[32m          selected2 = $.makeArray(selected);[m
[32m+[m[32m          selectCols(ignoreSelectable);[m
[32m+[m[32m          changeInput('columns_selected', selected2);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (selTarget === 'cell') {[m
[32m+[m[32m        var selected3 = [], selectable3 = [];[m
[32m+[m[32m        if (selected !== null) selected3 = selected;[m
[32m+[m[32m        if (selectable !== null && typeof selectable !== 'boolean') selectable3 = selectable;[m
[32m+[m[32m        var findIndex = function(ij, sel) {[m
[32m+[m[32m          for (var i = 0; i < sel.length; i++) {[m
[32m+[m[32m            if (ij[0] === sel[i][0] && ij[1] === sel[i][1]) return i;[m
[32m+[m[32m          }[m
[32m+[m[32m          return -1;[m
[32m+[m[32m        }[m
[32m+[m[32m         // Change selected3's value based on selectable3, then refresh the cell state[m
[32m+[m[32m        var onlyKeepSelectableCells = function() {[m
[32m+[m[32m          if (selDisable) { // users can't select; useful when only want backend select[m
[32m+[m[32m            selected3 = [];[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (selectable3.length === 0) return;[m
[32m+[m[32m          var nonselectable = selectable3[0][0] <= 0;[m
[32m+[m[32m          var out = [];[m
[32m+[m[32m          if (nonselectable) {[m
[32m+[m[32m            selected3.map(function(ij) {[m
[32m+[m[32m              // should make selectable3 positive[m
[32m+[m[32m              if (findIndex([-ij[0], -ij[1]], selectable3) === -1) { out.push(ij); }[m
[32m+[m[32m            });[m
[32m+[m[32m          } else {[m
[32m+[m[32m            selected3.map(function(ij) {[m
[32m+[m[32m              if (findIndex(ij, selectable3) > -1) { out.push(ij); }[m
[32m+[m[32m            });[m
[32m+[m[32m          }[m
[32m+[m[32m          selected3 = out;[m
[32m+[m[32m        }[m
[32m+[m[32m        // Change selected3's value based on selectable3, then[m
[32m+[m[32m        // refresh the cell selection state according to values in selected3[m
[32m+[m[32m        var selectCells = function(ignoreSelectable) {[m
[32m+[m[32m          if (!ignoreSelectable) onlyKeepSelectableCells();[m
[32m+[m[32m          table.$('td.' + selClass).removeClass(selClass);[m
[32m+[m[32m          if (selected3.length === 0) return;[m
[32m+[m[32m          if (server) {[m
[32m+[m[32m            table.cells({page: 'current'}).every(function() {[m
[32m+[m[32m              var info = tweakCellIndex(this);[m
[32m+[m[32m              if (findIndex([info.row, info.col], selected3) > -1)[m
[32m+[m[32m                $(this.node()).addClass(selClass);[m
[32m+[m[32m            });[m
[32m+[m[32m          } else {[m
[32m+[m[32m            selected3.map(function(ij) {[m
[32m+[m[32m              $(table.cell(ij[0] - 1, ij[1]).node()).addClass(selClass);[m
[32m+[m[32m            });[m
[32m+[m[32m          }[m
[32m+[m[32m        };[m
[32m+[m[32m        table.on('click.dt', 'tbody td', function() {[m
[32m+[m[32m          var $this = $(this), info = tweakCellIndex(table.cell(this));[m
[32m+[m[32m          if ($this.hasClass(selClass)) {[m
[32m+[m[32m            $this.removeClass(selClass);[m
[32m+[m[32m            selected3.splice(findIndex([info.row, info.col], selected3), 1);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            if (selMode === 'single') $(table.cells().nodes()).removeClass(selClass);[m
[32m+[m[32m            $this.addClass(selClass);[m
[32m+[m[32m            selected3 = selMode === 'single' ? [[info.row, info.col]] :[m
[32m+[m[32m              unique(selected3.concat([[info.row, info.col]]));[m
[32m+[m[32m          }[m
[32m+[m[32m          selectCells(false); // must call this to update selected3 based on selectable3[m
[32m+[m[32m          changeInput('cells_selected', transposeArray2D(selected3), 'shiny.matrix');[m
[32m+[m[32m        });[m
[32m+[m[32m        selectCells(false);  // in case users have specified pre-selected columns[m
[32m+[m[32m        changeInput('cells_selected', transposeArray2D(selected3), 'shiny.matrix');[m
[32m+[m
[32m+[m[32m        if (server) table.on('draw.dt', function(e) { selectCells(false); });[m
[32m+[m[32m        methods.selectCells = function(selected, ignoreSelectable) {[m
[32m+[m[32m          selected3 = selected ? selected : [];[m
[32m+[m[32m          selectCells(ignoreSelectable);[m
[32m+[m[32m          changeInput('cells_selected', transposeArray2D(selected3), 'shiny.matrix');[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // expose some table info to Shiny[m
[32m+[m[32m    var updateTableInfo = function(e, settings) {[m
[32m+[m[32m      // TODO: is anyone interested in the page info?[m
[32m+[m[32m      // changeInput('page_info', table.page.info());[m
[32m+[m[32m      var updateRowInfo = function(id, modifier) {[m
[32m+[m[32m        var idx;[m
[32m+[m[32m        if (server) {[m
[32m+[m[32m          idx = modifier.page === 'current' ? DT_rows_current : DT_rows_all;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          var rows = table.rows($.extend({[m
[32m+[m[32m            search: 'applied',[m
[32m+[m[32m            page: 'all'[m
[32m+[m[32m          }, modifier));[m
[32m+[m[32m          idx = addOne(rows.indexes().toArray());[m
[32m+[m[32m        }[m
[32m+[m[32m        changeInput('rows' + '_' + id, idx);[m
[32m+[m[32m      };[m
[32m+[m[32m      updateRowInfo('current', {page: 'current'});[m
[32m+[m[32m      updateRowInfo('all', {});[m
[32m+[m[32m    }[m
[32m+[m[32m    table.on('draw.dt', updateTableInfo);[m
[32m+[m[32m    updateTableInfo();[m
[32m+[m
[32m+[m[32m    // state info[m
[32m+[m[32m    table.on('draw.dt column-visibility.dt', function() {[m
[32m+[m[32m      changeInput('state', table.state());[m
[32m+[m[32m    });[m
[32m+[m[32m    changeInput('state', table.state());[m
[32m+[m
[32m+[m[32m    // search info[m
[32m+[m[32m    var updateSearchInfo = function() {[m
[32m+[m[32m      changeInput('search', table.search());[m
[32m+[m[32m      if (filterRow) changeInput('search_columns', filterRow.toArray().map(function(td) {[m
[32m+[m[32m        return $(td).find('input').first().val();[m
[32m+[m[32m      }));[m
[32m+[m[32m    }[m
[32m+[m[32m    table.on('draw.dt', updateSearchInfo);[m
[32m+[m[32m    updateSearchInfo();[m
[32m+[m
[32m+[m[32m    var cellInfo = function(thiz) {[m
[32m+[m[32m      var info = tweakCellIndex(table.cell(thiz));[m
[32m+[m[32m      info.value = table.cell(thiz).data();[m
[32m+[m[32m      return info;[m
[32m+[m[32m    }[m
[32m+[m[32m    // the current cell clicked on[m
[32m+[m[32m    table.on('click.dt', 'tbody td', function() {[m
[32m+[m[32m      changeInput('cell_clicked', cellInfo(this), null, null, {priority: 'event'});[m
[32m+[m[32m    })[m
[32m+[m[32m    changeInput('cell_clicked', {});[m
[32m+[m
[32m+[m[32m    // do not trigger table selection when clicking on links unless they have classes[m
[32m+[m[32m    table.on('mousedown.dt', 'tbody td a', function(e) {[m
[32m+[m[32m      if (this.className === '') e.stopPropagation();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    methods.addRow = function(data, rowname, resetPaging) {[m
[32m+[m[32m      var n = table.columns().indexes().length, d = n - data.length;[m
[32m+[m[32m      if (d === 1) {[m
[32m+[m[32m        data = rowname.concat(data)[m
[32m+[m[32m      } else if (d !== 0) {[m
[32m+[m[32m        console.log(data);[m
[32m+[m[32m        console.log(table.columns().indexes());[m
[32m+[m[32m        throw 'New data must be of the same length as current data (' + n + ')';[m
[32m+[m[32m      };[m
[32m+[m[32m      table.row.add(data).draw(resetPaging);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    methods.updateSearch = function(keywords) {[m
[32m+[m[32m      if (keywords.global !== null)[m
[32m+[m[32m        $(table.table().container()).find('input[type=search]').first()[m
[32m+[m[32m             .val(keywords.global).trigger('input');[m
[32m+[m[32m      var columns = keywords.columns;[m
[32m+[m[32m      if (!filterRow || columns === null) return;[m
[32m+[m[32m      filterRow.toArray().map(function(td, i) {[m
[32m+[m[32m        var v = typeof columns === 'string' ? columns : columns[i];[m
[32m+[m[32m        if (typeof v === 'undefined') {[m
[32m+[m[32m          console.log('The search keyword for column ' + i + ' is undefined')[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m[32m        // Update column search string and values on linked filter widgets.[m
[32m+[m[32m        // 'input' for factor and char filters, 'change' for numeric filters.[m
[32m+[m[32m        $(td).find('input').first().val(v).trigger('input', [true]).trigger('change');[m
[32m+[m[32m      });[m
[32m+[m[32m      table.draw();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    methods.hideCols = function(hide, reset) {[m
[32m+[m[32m      if (reset) table.columns().visible(true, false);[m
[32m+[m[32m      table.columns(hide).visible(false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    methods.showCols = function(show, reset) {[m
[32m+[m[32m      if (reset) table.columns().visible(false, false);[m
[32m+[m[32m      table.columns(show).visible(true);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    methods.colReorder = function(order, origOrder) {[m
[32m+[m[32m      table.colReorder.order(order, origOrder);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    methods.selectPage = function(page) {[m
[32m+[m[32m      if (table.page.info().pages < page || page < 1) {[m
[32m+[m[32m        throw 'Selected page is out of range';[m
[32m+[m[32m      };[m
[32m+[m[32m      table.page(page - 1).draw(false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    methods.reloadData = function(resetPaging, clearSelection) {[m
[32m+[m[32m      // empty selections first if necessary[m
[32m+[m[32m      if (methods.selectRows && inArray('row', clearSelection)) methods.selectRows([]);[m
[32m+[m[32m      if (methods.selectColumns && inArray('column', clearSelection)) methods.selectColumns([]);[m
[32m+[m[32m      if (methods.selectCells && inArray('cell', clearSelection)) methods.selectCells([]);[m
[32m+[m[32m      table.ajax.reload(null, resetPaging);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // update table filters (set new limits of sliders)[m
[32m+[m[32m    methods.updateFilters = function(newProps) {[m
[32m+[m[32m      // loop through each filter in the filter row[m
[32m+[m[32m      filterRow.each(function(i, td) {[m
[32m+[m[32m        var k = i;[m
[32m+[m[32m        if (filterRow.length > newProps.length) {[m
[32m+[m[32m          if (i === 0) return;  // first column is row names[m
[32m+[m[32m          k = i - 1;[m
[32m+[m[32m        }[m
[32m+[m[32m        // Update the filters to reflect the updated data.[m
[32m+[m[32m        // Allow "falsy" (e.g. NULL) to signify a no-op.[m
[32m+[m[32m        if (newProps[k]) {[m
[32m+[m[32m          setFilterProps(td, newProps[k]);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    table.shinyMethods = methods;[m
[32m+[m[32m  },[m
[32m+[m[32m  resize: function(el, width, height, instance) {[m
[32m+[m[32m    if (instance.data) this.renderValue(el, instance.data, instance);[m
[32m+[m
[32m+[m[32m    // dynamically adjust height if fillContainer = TRUE[m
[32m+[m[32m    if (instance.fillContainer)[m
[32m+[m[32m      this.fillAvailableHeight(el, height);[m
[32m+[m
[32m+[m[32m    this.adjustWidth(el);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // dynamically set the scroll body to fill available height[m
[32m+[m[32m  // (used with fillContainer = TRUE)[m
[32m+[m[32m  fillAvailableHeight: function(el, availableHeight) {[m
[32m+[m
[32m+[m[32m    // see how much of the table is occupied by header/footer elements[m
[32m+[m[32m    // and use that to compute a target scroll body height[m
[32m+[m[32m    var dtWrapper = $(el).find('div.dataTables_wrapper');[m
[32m+[m[32m    var dtScrollBody = $(el).find($('div.dataTables_scrollBody'));[m
[32m+[m[32m    var framingHeight = dtWrapper.innerHeight() - dtScrollBody.innerHeight();[m
[32m+[m[32m    var scrollBodyHeight = availableHeight - framingHeight;[m
[32m+[m
[32m+[m[32m    // we need to set `max-height` to none as datatables library now sets this[m
[32m+[m[32m    // to a fixed height, disabling the ability to resize to fill the window,[m
[32m+[m[32m    // as it will be set to a fixed 100px under such circumstances, e.g., RStudio IDE,[m
[32m+[m[32m    // or FlexDashboard[m
[32m+[m[32m    // see https://github.com/rstudio/DT/issues/951#issuecomment-1026464509[m
[32m+[m[32m    dtScrollBody.css('max-height', 'none');[m
[32m+[m[32m    // set the height[m
[32m+[m[32m    dtScrollBody.height(scrollBodyHeight + 'px');[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  // adjust the width of columns; remove the hard-coded widths on table and the[m
[32m+[m[32m  // scroll header when scrollX/Y are enabled[m
[32m+[m[32m  adjustWidth: function(el) {[m
[32m+[m[32m    var $el = $(el), table = $el.data('datatable');[m
[32m+[m[32m    if (table) table.columns.adjust();[m
[32m+[m[32m    $el.find('.dataTables_scrollHeadInner').css('width', '')[m
[32m+[m[32m        .children('table').css('margin-left', '');[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m  if (!HTMLWidgets.shinyMode) return;[m
[32m+[m
[32m+[m[32m  Shiny.addCustomMessageHandler('datatable-calls', function(data) {[m
[32m+[m[32m    var id = data.id;[m
[32m+[m[32m    var el = document.getElementById(id);[m
[32m+[m[32m    var table = el ? $(el).data('datatable') : null;[m
[32m+[m[32m    if (!table) {[m
[32m+[m[32m      console.log("Couldn't find table with id " + id);[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var methods = table.shinyMethods, call = data.call;[m
[32m+[m[32m    if (methods[call.method]) {[m
[32m+[m[32m      methods[call.method].apply(table, call.args);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      console.log("Unknown method " + call.method);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m})();[m
[32m+[m[32m</script>[m
