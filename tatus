[32m+[m[32m  theme(axis.text.y = element_text(colour = "darkred", size = 10)) +[m
[32m+[m[32m  theme(axis.text.y = element_text(size = 4)) +[m
[32m+[m[32m  scale_x_discrete(labels = wrap_format(25)) +[m
[32m+[m[32m  theme(axis.text.x = element_text(size = 8)) +[m
[32m+[m[32m  scale_y_continuous(limits = c(0, 20)) +[m
[32m+[m[32m  labs(title = str_wrap("Mycobacterium tuberculosis Counts at each Site", width = 30)) +[m
