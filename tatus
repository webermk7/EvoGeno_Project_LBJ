[32m+[m[32m  theme(legend.key.width = unit(0.1, 'cm')) +[m
[32m+[m[32m  theme(legend.title = element_text(colour = "black", size = 2, face = "bold")) +[m
[32m+[m[32m  theme(legend.text = element_text(colour = "black", size = 2)) +[m
[32m+[m[32m  theme(legend.box.background = element_rect()) +[m
